	keywords = {Animals, Horses, Dogs, Cats, Veterinary Medicine/standards, Blood Specimen Collection/methods, Blood Specimen Collection/standards, Blood Specimen Collection/veterinary, Cats/blood, Dogs/blood, Horses/blood, Reference Standards, Thrombelastography/instrumentation, Thrombelastography/methods, Thrombelastography/veterinary},
