	note = {Edition: 2013/11/19},
	keywords = {Animals, Cattle, Female, Energy Metabolism, Lactation, Cattle/physiology, Cattle Diseases/prevention \& control, Dairying/methods, Milk/secretion, Fertility, Milk Proteins, Milk Proteins/metabolism},
	annote = {last updated in refbase Mon, 11 Aug 2014 10:55:35 +0100},
