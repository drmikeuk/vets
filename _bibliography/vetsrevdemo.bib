	author = {Varela, N P and Dick, P and Wilson, J},
	year = {2013},
	note = {Edition: 2012/08/04},
	keywords = {Animals, Cattle, Cattle Diseases/prevention \& control, Escherichia coli Infections/veterinary, Cattle Diseases/microbiology, Escherichia coli Vaccines, Escherichia coli Infections/prevention \& control, Escherichia coli O157/immunology, Escherichia coli Vaccines/immunology},
