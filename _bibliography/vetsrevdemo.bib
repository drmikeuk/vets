	note = {Edition: 2018/02/20},
	keywords = {Cattle, Goats, Sheep, Dogs, Humans, Rodents, Seroprevalence, Bovines, Leptospirosis, Amphibians, Caprines, Ovines, Canines, Caribbean},
	pages = {34--42},
	annote = {EBSCOhost, Wiley-Blackwell, Springer Link, Science Direct, Wiley Online and PubMed Central searched},
