@article{ohaire_animal-assisted_2015,
	title = {Animal-{Assisted} {Intervention} for trauma: a systematic literature review},
	volume = {6},
	issn = {1664-1078},
	url = {http://dx.doi.org/10.3389/fpsyg.2015.01121},
