	year = {2013},
	note = {Edition: 2012/07/24},
	keywords = {Animals, Cattle, Female, Male, Animal Husbandry/methods, Cattle/physiology, Models, Theoretical, Cattle/growth \& development, Dairying/methods, Animal Husbandry/economics, Dairying/economics, Kenya, Reference Values},
