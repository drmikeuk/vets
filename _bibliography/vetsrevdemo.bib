	journal = {Livestock Science},
	author = {Lean, I J and Westwood, C T and Golder, H M and Vermunt, J J},
	year = {2013},
	keywords = {cattle, reviews, effects, interactions, research, lactic acid, diets, feeding, diet, intestines, health, rumen, fibre, carbohydrates, lameness, lesions, nutrients, starch, [Indexed using CAB Thesaurus terms], animals, Chordata, eukaryotes, vertebrates, Artiodactyla, Bos, Bovidae, mammals, ruminants, Animal Nutrition (General) [LL500], studies, body condition, lactate, Animal Nutrition (Physiology) [LL510], fiber, acidosis, biotin, claw, coenzymes, endotoxins, fats, Food Science and Food Products (Human) [QQ000], histamine, lipids, lipins, metabolic acidosis, microelements, mineral nutrition, minerals, monensin, peroxidation, precursors, ratios, rumensin, saccharides, toxins, trace elements, vitamin B, vitamin B complex, vitamins},
