	keywords = {Animals, Cattle, Female, Animal Nutritional Physiological Phenomena, Milk/chemistry, Digestion, Cattle/metabolism, Dietary, Dietary/metabolism, Phosphorus, Lactation/physiology, Bayes Theorem, Dietary Fiber/administration \& dosage, Dietary Proteins, Dietary Proteins/administration \& dosage, Dietary/administration \& dosage, Dietary/pharmacokinetics, Feces/chemistry, Phosphorus/analysis},
