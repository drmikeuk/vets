	number = {1},
	journal = {PLoS One},
	author = {Van Kerkhove, M D and Mumford, E and Mounts, A W and Bresee, J and Ly, S and Bridges, C B and Otte, J},
	year = {2011},
	note = {Edition: 2011/02/02},
	keywords = {Animals, Humans, Chickens, Poultry, Birds, Influenza, Influenza A Virus, Disease Vectors, H5N1 Subtype/pathogenicity, Human/virology, Influenza in Birds/transmission},
	annote = {last updated in refbase Wed, 04 Dec 2013 14:23:22 +0000},
