	title = {Use of biodiesel co-products (glycerol) as alternative sources of energy in animal nutrition: a systematic review},
	volume = {46},
	url = {http://www.scielo.cl/scielo.php?script=sci_issues&pid=0301-732X&lng=en&nrm=iso http://gateway.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20143236355 http://sfx.nottingham.ac.uk:80/sfx_local?genre=article&atitle=Use+of+biodiesel+co-pro},
