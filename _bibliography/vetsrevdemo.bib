	keywords = {Animals, Horses, Dogs, Administration, Anti-Bacterial Agents/administration \& dosage/therapeutic use, Antifungal Agents/administration \& dosage/therapeutic use, Bacterial/drug therapy/veterinary, Dermatomycoses/drug therapy/veterinary, Skin Diseases, Topical, Yeasts},
