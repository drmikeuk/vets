	keywords = {Cattle},
	pages = {167--75; quiz 176},
	annote = {last updated in refbase Thu, 29 Aug 2013 14:38:54 +0100},
	annote = {PubMed and MEDLINE searched},
	annote = {UoN @ douglas.grindlay @ 826},
}

