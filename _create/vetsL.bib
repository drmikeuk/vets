	keywords = {Animals, Treatment, Memory, Cognition, Exercise, Disease Models, Animal, Stress, Physical Conditioning, Animal/*psychology, Maze Learning/*physiology, Memory Disorders/etiology/prevention \& control/psychology/*therapy, Physical activity, Preventive, Psychological, Psychophysiological, Rescue, Stress, Psychological/*complications/psychology, Therapeutic},
	pages = {1--12},
	annote = {1880-6562Loprinzi, Paul DFrith, EmilyJournal ArticleReviewJapanJ Physiol Sci. 2019 Jan;69(1):1-12. doi: 10.1007/s12576-018-0638-0. Epub 2018 Sep 10.},
