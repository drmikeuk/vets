	pages = {e17535},
	annote = {1536-5964Liu, WeiFeng, Rui-ZhiJiang, Hong-LiJournal ArticleMeta-AnalysisSystematic ReviewMedicine (Baltimore). 2019 Oct;98(41):e17535. doi: 10.1097/MD.0000000000017535.},
