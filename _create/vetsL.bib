	pages = {128},
	annote = {1472-6831Lee, Jeong-HeeHan, Sang-SunOrcid: 0000-0003-1775-7862Lee, ChenaKim, Young HyunBattulga, Bulgan2017R1D1A1B03033218/National Research Foundation of Korea/InternationalJournal ArticleMeta-AnalysisSystematic ReviewBMC Oral Health. 2019 Jun 26;19(1):128. doi: 10.1186/s12903-019-0799-0.},
