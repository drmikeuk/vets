	keywords = {Animals, Disease Reservoirs, Horses/*parasitology, Horse Diseases/*parasitology, Leishmania/classification/*isolation \& purification, Leishmaniasis/*veterinary},
	pages = {574--581},
	annote = {1984-2961Limeira, Clécio HenriqueOrcid: 0000-0002-3151-3680Alves, Clebert JoséAzevedo, Sérgio Santos deSantos, Carolina de Souza Américo BatistaMelo, Marcia Almeida deSoares, Rafael RodriguesBarnabé, Nathanael Natércio da CostaRodrigues, Gabriel de QueirozJournal ArticleMeta-AnalysisSystematic ReviewBrazilRev Bras Parasitol Vet. 2019 Oct-Dec;28(4):574-581. doi: 10.1590/S1984-29612019074.},
