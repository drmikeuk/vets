	note = {Edition: 2018/02/06},
	keywords = {Animals, Humans, Mice, Animal model, Cell Proliferation, Coculture Techniques, Mice, Inbred C57BL, Cell Line, Tumor, Melanoma, Mesenchymal Stem Cells/*cytology, Cell- and Tissue-Based Therapy/methods, Cell therapy, Cocarcinogenesis/*pathology, Melanoma, Experimental/*pathology/therapy, Pre-clinical trial},
	pages = {231--240},
	annote = {1432-069xCordeiro, Marcos FreitasMarmitt, Luana PatriciaHorn, Ana PaulaJournal ArticleMeta-AnalysisReviewSystematic ReviewGermanyArch Dermatol Res. 2018 Apr;310(3):231-240. doi: 10.1007/s00403-018-1819-7. Epub 2018 Feb 3.},
