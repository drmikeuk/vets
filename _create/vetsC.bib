	keywords = {Animals, Humans, Rats, Female, Disease Models, Animal, *Osteoporosis, Postmenopausal, *Ovariectomy, *Vibration, Fracture Healing, Osteoporotic Fractures/*therapy, Physical Therapy Modalities},
	pages = {677--686},
	annote = {1530-0374Chen, JinmanRuan, HongfengLiu, YangBao, JiaminXu, HaoYao, MinCui, XuejunLiang, QianqianWang, YongjunJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesMenopause. 2018 Dec 17;26(6):677-686. doi: 10.1097/GME.0000000000001285.},
