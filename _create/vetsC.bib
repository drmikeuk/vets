	note = {Edition: 2020/09/04},
	keywords = {pain, in the collection, analyses, or interpretation of data, in the writing, of the manuscript, or in the decision to publish the results., of the study, equine-assisted therapy, hippotherapy, horse-riding, horseback riding, simulator},
	annote = {1648-9144Collado-Mateo, DanielLavín-Pérez, Ana MyriamOrcid: 0000-0001-9357-9987Fuentes García, Juan PedroOrcid: 0000-0002-8299-1092García-Gordillo, Miguel ÁngelOrcid: 0000-0003-1736-0996Villafaina, SantosOrcid: 0000-0003-0784-1753Journal ArticleReviewMedicina (Kaunas). 2020 Aug 31;56(9):444. doi: 10.3390/medicina56090444.},
