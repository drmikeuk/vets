	journal = {Phytomedicine},
	author = {Carvalho, A. M. S. and Heimfarth, L. and Santos, K. A. and Guimarães, A. G. and Picot, L. and Almeida, Jrgs and Quintans, J. S. S. and Quintans-Júnior, L. J.},
