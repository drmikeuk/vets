	keywords = {Animals, Cattle, Goats, Sheep, China, Livestock, Pigs, Foot-and-Mouth Disease Virus/physiology, Foot-and-Mouth Disease/prevention \& control, Foot-and-Mouth Disease/virology, Vaccination/veterinary},
	pages = {94--100},
	annote = {last updated in refbase Wed, 11 Feb 2015 11:46:40 +0000},
	annote = {UoN @ douglas.grindlay @ 1219},
	annote = {Wan Fang Data, Wei Pu Information, China National Knowledge Infrastructure and PubMed searched},
