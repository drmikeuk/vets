	note = {Edition: 2018/03/24},
	keywords = {Animals, Humans, *Cytokines, Cytokines/*metabolism, Biomarkers/*metabolism, *cd40l, *il-1β, *il-6, *Neuro-inflammation, *Prognostication, *Substance P, *tgf-β, *timp-1, *tnf-α, *Traumatic brain injury, Brain Injuries, Traumatic/*diagnosis/immunology/metabolism},
	pages = {22--32},
	annote = {1556-0961Casault, ColinOrcid: 0000-0002-3032-1206Al Sultan, Abdulaziz SBanoei, MohammadCouillard, PhilippeKramer, AndreasWinston, Brent WJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesNeurocrit Care. 2019 Feb;30(1):22-32. doi: 10.1007/s12028-018-0522-z.},
}

@article{casavechia_systematic_2018,
	title = {Systematic review and meta-analysis on {Schistosoma} mansoni infection prevalence, and associated risk factors in {Brazil}},
	volume = {145},
	issn = {0031-1820},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20193019653 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Systematic+review+and+meta-analysis+on+Schistosoma+mansoni+infection+prevalence%2C+and+associated+risk+factors+in+Brazil.&title=Parasitology&issn=0031-1820&date=2018&volume=145&issue=8&spage=1000&aulast=Casavechia%2C+M.+T.+G.&isbn=&__char_set=utf8},
	doi = {10.1017/S0031182017002268},
	abstract = {We performed a systematic review and meta-analysis on the prevalence and factors associated with Schistosoma mansoni infection in Brazil. We searched the PubMed, Web of Science and Latin-American and Caribbean System on Health Sciences Information (LILACS) databases, scientific publications articles, according to The PRISMA Statement, from 2000 to 2016. A total of 27 studies were included according to the established criteria. The prevalence of S. mansoni infection varied widely, from 0.1 to 73.1\%, based on Kato-Katz technique. Of the identified studies, 42.9\% were performed in the state of Minas Gerais, and 33.3\% were performed in the northeast region of Brazil. We identified sex, age, education level, family income, contact with water and the presence of the intermediate host snail as major risk factors associated with infection. The meta-analysis summarized a high prevalence rate pooled for Schistosoma mansoni. On the other hand, the analysis of the subgroup showed a highly significant reduction of the prevalence rate after control measures. The epidemiological factors evidenced in the studies show the influence of environmental and social conditions on the occurrence of schistosomiasis.},
	language = {English},
	number = {8},
	journal = {Parasitology},
	author = {Casavechia, M. T. G. and Melo, G. de A. N. de and Fernandes, A. C. B. da S. and Castro, K. R. de and Pedroso, R. B. and Santos, T. da S. and Teixeira, J. J. V.},
