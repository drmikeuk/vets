	keywords = {Animals, Cattle, Prevalence, Meta-analysis, Cryptosporidium, Dairy cattle, China/epidemiology, Databases, Factual, Cattle Diseases/*epidemiology/parasitology, Geographic Mapping, Cryptosporidiosis/*epidemiology/parasitology, Cryptosporidium/*isolation \& purification, Diarrhea/epidemiology/parasitology, Mainland China},
