	keywords = {Animals, Risk Assessment, Ecosystem, Amphibians, Environmental Pollutants/toxicity, Glycine/analogs \& derivatives/toxicity, Herbicides/toxicity, Larva/drug effects/growth \& development},
	pages = {1688--1700},
	annote = {last updated in refbase Wed, 04 Dec 2013 14:27:47 +0000},
	annote = {UoN @ douglas.grindlay @ 1172},
