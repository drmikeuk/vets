	author = {Wilson, D V and Pascoe, P J},
	year = {2016},
	note = {Edition: 2015/10/13},
	keywords = {Cats, Feline, Analgesia, Pain, Onychectomy},
	pages = {5--17},
	annote = {last updated in refbase Wed, 10 Feb 2016 13:51:57 +0000},
	annote = {National Library of Medicine, EMBASE and CAB International searched},
