	pages = {300060519845488},
	annote = {1473-2300Wang, Wen-HungErazo, Esmeralda MerariIshcol, Max R ChangLin, Chih-YenAssavalapsakul, WanchaiThitithanyanont, AruneeWang, Sheng-FanOrcid: 0000-0002-4183-3872Journal ArticleSystematic ReviewJ Int Med Res. 2020 Jan;48(1):300060519845488. doi: 10.1177/0300060519845488. Epub 2019 May 8.},
