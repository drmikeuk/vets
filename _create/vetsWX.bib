	journal = {Occupational Therapy International},
	author = {Winkle, M and Crowe, T K and Hendrix, I},
	year = {2012},
	note = {Edition: 2011/08/23},
	keywords = {Animals, Dogs, Humans, Evidence-Based Practice, Disabled Persons/psychology, Mobility Limitation, Occupational Therapy, Social Participation/psychology},
	pages = {54--66},
