	keywords = {Animals, Humans, Rodentia, Growth Hormone/metabolism/*pharmacology, *Pain, *Ghrelin, *Growth Hormone, *Insulin-Like Growth Factor 1, *Nociception, *Pharmacology, Chronic Pain/drug therapy/metabolism/physiopathology, Hyperalgesia/*drug therapy/*metabolism/*physiopathology},
	pages = {21--31},
	annote = {1526-4637Xu, JijunCasserly, ElizabethYin, YanCheng, JianguoJournal ArticleSystematic ReviewEnglandPain Med. 2020 Jan 1;21(1):21-31. doi: 10.1093/pm/pny280.},
