	number = {2},
	journal = {Zhongguo Xue Xi Chong Bing Fang Zhi Za Zhi},
	author = {Wang, L. P. and Deng, W. P. and Jia, T. W. and Qin, Z. Q. and Xu, J.},
	month = apr,
