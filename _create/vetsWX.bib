	pages = {10--21},
	annote = {1873-2232Wang, ZhongLiu, Bao ShengWang, Xiao YingWei, Qing HuiTian, HuanWang, Li QiJournal ArticleMeta-AnalysisReviewNetherlandsAnim Reprod Sci. 2018 Oct;197:10-21. doi: 10.1016/j.anireprosci.2018.08.035. Epub 2018 Aug 30.},
