	keywords = {pain, methods, reliability, validity, mouse grimace scale},
	annote = {2076-2615Whittaker, Alexandra LOrcid: 0000-0001-9011-8296Liu, YifanBarker, Timothy HOrcid: 0000-0002-6897-814xAPP1140072/National Health and Medical Research Council/Journal ArticleReviewAnimals (Basel). 2021 Mar 3;11(3):673. doi: 10.3390/ani11030673.},
}

@article{wijesinghe_intrinsic_2020,
	title = {The intrinsic plasticity of medial vestibular nucleus neurons during vestibular compensation - a systematic review and meta-analysis},
	volume = {9},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba7&AN=20203362850 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=The+intrinsic+plasticity+of+medial+vestibular+nucleus+neurons+during+vestibular+compensation+-+a+systematic+review+and+meta-analysis.&title=Systematic+Reviews&issn=2046-4053&date=2020&volume=9&issue=145&spage=&aulast=Wijesinghe%2C+R.&isbn=&__char_set=utf8},
	doi = {10.1186/s13643-020-01399-2},
	abstract = {Background: Vestibular compensation is a homeostatic process that occurs in the central nervous system in response to peripheral vestibular dysfunction. Experimental studies in rodent models have suggested that unilateral peripheral vestibular lesions are correlated with an increase in the intrinsic excitability of central vestibular neurons. This process may be dependent on the intrinsic properties of the neurons themselves. We aimed to conduct a systematic review of the literature to survey the evidence for changes in intrinsic plasticity observed during the acute phase of vestibular compensation.},
	language = {English},
	number = {145},
	journal = {Systematic Reviews},
	author = {Wijesinghe, R. and Camp, A.},
