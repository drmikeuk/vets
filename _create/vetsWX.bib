	keywords = {Animals, Cattle, Female, Anti-Bacterial Agents/*therapeutic use, *Lactation, *Network Meta-Analysis, *Dairy cow, *mixed treatment comparison, *udder health, Mastitis, Bovine/*drug therapy},
	pages = {229--246},
	annote = {1475-2654Winder, C BOrcid: 0000-0002-7314-3657Sargeant, J MOrcid: 0000-0003-2420-1766Hu, DWang, COrcid: 0000-0003-4489-4344Kelton, D FGodkin, M AOrcid: 0000-0002-7154-5791Churchill, K JO'Connor, A MOrcid: 0000-0003-0604-7822Journal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewEnglandAnim Health Res Rev. 2019 Dec;20(2):229-246. doi: 10.1017/S1466252319000318.},
