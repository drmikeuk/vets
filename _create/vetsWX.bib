@article{wolfger_systematic_2015,
	title = {A {Systematic} {Review} of {Bovine} {Respiratory} {Disease} {Diagnosis} {Focused} on {Diagnostic} {Confirmation}, {Early} {Detection}, and {Prediction} of {Unfavorable} {Outcomes} in {Feedlot} {Cattle}},
