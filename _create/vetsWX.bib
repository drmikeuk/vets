	note = {Edition: 2019/06/04},
	keywords = {Animals, Humans, *Pets, *veterinary, *companion/small animals, *hand hygiene, *infection control, *nosocomial, *personal protective equipment, *sharps management, Disinfection, Hand Hygiene, Hospitals, Animal/*standards, Infection Control/*methods/*standards, Needlestick Injuries/prevention \& control},
	pages = {439--457},
