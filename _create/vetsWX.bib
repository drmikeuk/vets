@article{wang_down-regulated_2018,
	title = {Down-{Regulated} {microRNA}-34a {Expression} as a {Prognostic} {Marker} for {Poor} {Osteosarcoma} in {Mice}: {A} {Systematic} {Review} and {Meta}-{Analysis}},
