	pages = {331--341},
	annote = {1863-2378Choe, Young JuneOrcid: 0000-0003-2733-0715Teevan, BridgetSmit, MichaelQuilliam, DanielaBandy, UtpalaMermel, LeonardJournal ArticleSystematic ReviewGermanyZoonoses Public Health. 2020 Jun;67(4):331-341. doi: 10.1111/zph.12706. Epub 2020 Apr 29.},
