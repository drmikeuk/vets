	note = {Edition: 2018/12/12},
	keywords = {Animals, Humans, wounds, essential oils, Anti-Infective Agents/pharmacology, Oxidative Stress/drug effects, Cytokines/metabolism, wound healing, Wound Healing/*drug effects, Monoterpenes/*pharmacology, carvacrol, Oils, Volatile/*pharmacology, Cymenes, thymol, Thymol/pharmacology},
	pages = {141--155},
	annote = {2042-7158Costa, Michelle FonsecaDurço, Aimée ObolariRabelo, Thallita KellyBarreto, Rosana de Souza SiqueiraGuimarães, Adriana GibaraOrcid: 0000-0003-1643-5642Journal ArticleSystematic ReviewEnglandJ Pharm Pharmacol. 2019 Feb;71(2):141-155. doi: 10.1111/jphp.13054. Epub 2018 Dec 7.},
}

