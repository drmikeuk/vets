	keywords = {Prevalence, China, Meta-analysis, Systematic review, Bovine viral diarrhea virus, interest., Yaks},
	pages = {104185},
	annote = {1096-1208Diao, Nai-ChaoGong, Qing-LongLi, Jian-MingZhao, DanLi, DongZhao, BoGe, Gui-YangLi, Dong-LiShi, KunDu, RuiJournal ArticleReviewEnglandMicrob Pathog. 2020 Jul;144:104185. doi: 10.1016/j.micpath.2020.104185. Epub 2020 Apr 6.},
