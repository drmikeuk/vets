	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba7&AN=20203348222 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Occurrence+of+aflatoxin+M1+in+milks+of+five+animal+species+in+Iran%3A+a+systematic+review+and+meta-analysis.&title=Food+Reviews+International&issn=8755-9129&date=2020&volume=36&issue=7&spage=692&aulast=Bahraman%2C+A.+G.&isbn=&__char_set=utf8},
