	pages = {298--306},
	annote = {1879-0712da L D Barros, ManuellaManhães-de-Castro, RaulAlves, Daniele TQuevedo, Omar GuzmánToscano, Ana ElisaBonnin, AlexandreGalindo, LigiaJournal ArticleReviewSystematic ReviewNetherlandsEur J Pharmacol. 2018 Aug 15;833:298-306. doi: 10.1016/j.ejphar.2018.06.013. Epub 2018 Jun 8.},
