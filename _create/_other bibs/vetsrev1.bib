	note = {Edition: 2018/04/20},
	keywords = {Animals, Cattle, Humans, Chickens, Female, Pregnancy, Swine, Adult, Sensitivity and Specificity, China/epidemiology, Bayes Theorem, Seroepidemiologic Studies, Immunoassay, *Pregnancy Complications, *Toxoplasma/immunology, Antibodies, Protozoan, Geography, Medical, Livestock/*parasitology, Parasitic Diseases, Animal/*epidemiology/immunology/*parasitology, Public Health Surveillance, Toxoplasmosis/*epidemiology/immunology/*parasitology},
	pages = {6218},
	annote = {2045-2322Deng, HuifangOrcid: 0000-0002-4880-760xDevleesschauwer, BrechtOrcid: 0000-0002-2867-6892Liu, MingyuanLi, JianhuaWu, Yongningvan der Giessen, Joke W BOpsteegh, MariekeJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewSci Rep. 2018 Apr 18;8(1):6218. doi: 10.1038/s41598-018-24361-8.},
