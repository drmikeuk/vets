	keywords = {Animals, Humans, Female, Male, Child, Aged, Adolescent, Adult, Middle Aged, Young Adult, Aged, 80 and over, Diagnosis, Differential, *Echinococcus, Anthelmintics/therapeutic use, Benzimidazoles/therapeutic use, Echinococcosis/*complications/diagnosis/therapy, Neglected Diseases},
	pages = {e10671},
