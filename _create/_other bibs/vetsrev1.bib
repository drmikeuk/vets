	keywords = {Animals, Humans, Randomized Controlled Trials as Topic, Disease Models, Animal, Pain, Apoptosis/physiology, Annulus fibrosus, Connective Tissue/pathology/physiopathology, Discopathy, Intervertebral disc, Intervertebral Disc Degeneration/pathology/*physiopathology/therapy, Intervertebral Disc/*pathology/*physiopathology, Nucleus pulposus, Spine},
	pages = {265--273},
	annote = {1878-8769Amelot, AymericMazel, ChristianJournal ArticleSystematic ReviewUnited StatesWorld Neurosurg. 2018 Dec;120:265-273. doi: 10.1016/j.wneu.2018.09.032. Epub 2018 Sep 12.},
