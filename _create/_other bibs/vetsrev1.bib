	author = {Alberton Nuernberg, M. A. and Janjacomo Miessi, D. M. and Ivanaga, C. A. and Bocalon Olivo, M. and Ervolino, E. and Gouveia Garcia, V. and Wainwright, M. and Theodoro, L. H.},
