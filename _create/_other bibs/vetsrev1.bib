	keywords = {Animals, Dogs, veterinary, Cats, Anticoagulants/therapeutic use, Heparin/therapeutic use, Critical Care, administration and dosage, anticoagulant, antiplatelet agents, Cat Diseases/*drug therapy, Clinical Protocols/standards, Curative, Dog Diseases/*drug therapy, Fibrinolytic Agents/*therapeutic use, Practice Patterns, Physicians'/standards, Veterinary Medicine/*standards},
	pages = {60--74},
	annote = {1476-4431Blais, Marie-ClaudeOrcid: 0000-0003-0877-4425Bianco, DomenicoGoggs, RobertOrcid: 0000-0001-7446-6987Lynch, Alex MPalmer, LeeOrcid: 0000-0002-3849-1736Ralph, AlanSharp, Claire ROrcid: 0000-0002-1797-9783Consensus Development ConferenceJournal ArticlePractice GuidelineSystematic ReviewUnited StatesJ Vet Emerg Crit Care (San Antonio). 2019 Jan;29(1):60-74. doi: 10.1111/vec.12795.},
