	keywords = {Animals, Humans, Transcriptome, *systematic review, MicroRNAs/genetics, Epigenesis, Genetic, *arbovirus, *histone modifications, *host epigenetics, *microRNAs, Arbovirus Infections/*genetics, Arboviruses/*physiology, Arthropod Vectors/*physiology, Arthropods/*physiology},
	pages = {1207},
	annote = {1664-3224de Aguiar, Gabriella Pequeno Costa GomesLeite, Claudio Manuel Gonçalves da SilvaDias, BeatrizVasconcelos, Silvania Maria Mendesde Moraes, Renata Amaralde Moraes, Maria Elisabete AmaralVallinoto, Antonio Carlos RosarioMacedo, Danielle SilveiraCavalcanti, Luciano Pamplona de GoesMiyajima, FabioJournal ArticleSystematic ReviewFront Immunol. 2019 May 31;10:1207. doi: 10.3389/fimmu.2019.01207. eCollection 2019.},
