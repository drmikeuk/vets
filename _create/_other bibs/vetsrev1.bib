	title = {The association between sub-therapeutic antibiotics and {Salmonella} {Typhimurium} in market-weight swine: a systematic review and summation of evidence from 1950 to 2007},
	volume = {57},
	issn = {1863-2378},
	url = {http://dx.doi.org/10.1111/j.1863-2378.2010.01331.x},
