	keywords = {Animals, Humans, Zoonoses, Anti-Bacterial Agents/therapeutic use, *brucellosis, *congenital infection, *intrauterine infection, Brucellosis/*congenital/drug therapy/epidemiology/mortality},
	pages = {393--403},
	annote = {1557-7759Alsaif, ManalDabelah, KamalGirim, HeshamFeatherstone, RobinRobinson, Joan LJournal ArticleResearch Support, Non-U.S. Gov'tReviewSystematic ReviewUnited StatesVector Borne Zoonotic Dis. 2018 Aug;18(8):393-403. doi: 10.1089/vbz.2018.2280. Epub 2018 Jun 29.},
