	note = {Edition: 2019/12/16},
	keywords = {Animals, Humans, Regenerative Medicine/methods, Wound Healing/*physiology, *inflammation, Chronic Disease, *growth factors, Cytokines/physiology, *failed healing response, *tendinopathy, Intercellular Signaling Peptides and Proteins/physiology, Tendinopathy/*physiopathology, Tendon Injuries/*physiopathology/therapy},
	pages = {97--116},
	annote = {1471-8391Chisari, EmanueleRehak, LauraKhan, Wasim SMaffulli, NicolaJournal ArticleSystematic ReviewEnglandBr Med Bull. 2019 Dec 11;132(1):97-116. doi: 10.1093/bmb/ldz035.},
