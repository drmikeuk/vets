	keywords = {Animals, Humans, Swine, Animals, Wild, *Models, Biological, World Health Organization, Cysticercosis/transmission/*veterinary, Disease Eradication, Infection Control, Swine Diseases/parasitology/*transmission, Taenia solium, Taeniasis/transmission/*veterinary, Zoonoses/parasitology/*transmission},
	pages = {e0007301},
