	keywords = {Animals, Humans, *Education, Veterinary, assessment, medicine, Clinical Competence, *Decision Making, *Workplace, clinical science, communications, Competency-Based Education, competency/competencies, Curriculum, educational methods, Feedback, problem-based learning, veterinary teaching hospital},
	pages = {340--352},
	annote = {Duijn, Chantal C M ADijk, Emma J vanMandoki, MiraBok, Harold G JCate, Olle Th J TenJournal ArticleSystematic ReviewCanadaJ Vet Med Educ. 2019 Fall;46(3):340-352. doi: 10.3138/jvme.0917-123r. Epub 2019 Feb 26.},
