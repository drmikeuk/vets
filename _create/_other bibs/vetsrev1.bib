	pages = {256--262},
	annote = {2047-7732Chang, EunsukOrcid: 0000-0002-1350-3606Journal ArticleSystematic ReviewPathog Glob Health. 2019 Sep;113(6):256-262. doi: 10.1080/20477724.2019.1683326. Epub 2019 Oct 28.},
