	pages = {3085--3096},
	annote = {1549-7852Belluco, SimoneOrcid: 0000-0002-3764-4040Simonato, GiuliaMancin, MarziaPietrobelli, MarioRicci, AntoniaJournal ArticleMeta-AnalysisSystematic ReviewUnited StatesCrit Rev Food Sci Nutr. 2018;58(18):3085-3096. doi: 10.1080/10408398.2017.1352563. Epub 2017 Oct 11.},
