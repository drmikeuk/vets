	note = {Edition: 2020/10/06},
	keywords = {Animals, Sheep, Australia, New Zealand, Sheep, Domestic, animal production, disease, impact, *Meat, *Red Meat, health conditions, meat value chain},
	pages = {32--39},
	annote = {1751-0813Dal Grande, EOrcid: 0000-0002-5919-3893Caraguel, CLee, S JNielsen, T DP.PSH.0852/Meat and Livestock Australia/Journal ArticleReviewSystematic ReviewEnglandAust Vet J. 2021 Jan;99(1-2):32-39. doi: 10.1111/avj.13026. Epub 2020 Oct 5.},
