	keywords = {Animals, Humans, Disease Models, Animal, Mesenchymal Stem Cells/*metabolism, Cell- and Tissue-Based Therapy/*methods, Pancreatitis, Chronic/genetics/metabolism/*therapy},
	pages = {3250864},
	annote = {1942-0994Ahmed, Sara MMorsi, MahmoudOrcid: 0000-0002-6621-0751Ghoneim, Nehal IAbdel-Daim, Mohamed MOrcid: 0000-0002-4341-2713El-Badri, NagwaOrcid: 0000-0002-1965-611xJournal ArticleReviewSystematic ReviewOxid Med Cell Longev. 2018 Mar 18;2018:3250864. doi: 10.1155/2018/3250864. eCollection 2018.},
