	number = {4},
	journal = {PLoS Negl Trop Dis},
	author = {Dixon, M. A. and Braae, U. C. and Winskill, P. and Walker, M. and Devleesschauwer, B. and Gabriël, S. and Basáñez, M. G.},
	month = apr,
	year = {2019},
