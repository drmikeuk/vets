	pages = {1488--1500},
	annote = {1099-1573Avila, CatharineOrcid: 0000-0001-8408-8369Whitten, DawnEvans, SueJournal ArticleReviewSystematic ReviewEnglandPhytother Res. 2018 Aug;32(8):1488-1500. doi: 10.1002/ptr.6099. Epub 2018 Apr 30.},
}

