	number = {1},
	journal = {J Vet Emerg Crit Care (San Antonio)},
	author = {Blais, M. C. and Bianco, D. and Goggs, R. and Lynch, A. M. and Palmer, L. and Ralph, A. and Sharp, C. R.},
	month = jan,
	year = {2019},
