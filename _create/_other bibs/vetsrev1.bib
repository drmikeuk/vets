	keywords = {Animals, Risk Factors, Disease Models, Animal, Behavior, Animal, Publication Bias, Animal Husbandry, Publications, Antineoplastic Agents/administration \& dosage/*adverse effects, Drug Administration Routes, Outcome Assessment, Health Care, Peripheral Nervous System Diseases/*chemically induced},
	pages = {e3000243},
	annote = {1545-7885Currie, Gillian LOrcid: 0000-0003-3052-2929Angel-Scott, Helena NOrcid: 0000-0002-7109-920xColvin, LesleyOrcid: 0000-0002-1563-8600Cramond, FalaHair, KaitlynOrcid: 0000-0003-0180-7343Khandoker, LailaLiao, JingMacleod, MalcolmOrcid: 0000-0001-9187-9839McCann, Sarah KOrcid: 0000-0003-4737-2349Morland, RosieOrcid: 0000-0002-1881-0461Sherratt, NickiStewart, RobertTanriver-Ayder, EzgiOrcid: 0000-0002-9653-2540Thomas, JamesOrcid: 0000-0003-4805-4190Wang, QianyingWodarski, RachelOrcid: 0000-0002-1254-9418Xiong, RanRice, Andrew S CSena, Emily SOrcid: 0000-0002-3282-8502Journal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewPLoS Biol. 2019 May 20;17(5):e3000243. doi: 10.1371/journal.pbio.3000243. eCollection 2019 May.},
