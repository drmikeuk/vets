	pages = {335--342},
	annote = {1557-7759Dalvi, Ana Paula RazalCarvalho, Thaiza Dutra Gomes deWerneck, Guilherme LoureiroJournal ArticleResearch Support, Non-U.S. Gov'tReviewSystematic ReviewUnited StatesVector Borne Zoonotic Dis. 2018 Jul;18(7):335-342. doi: 10.1089/vbz.2017.2162. Epub 2018 Apr 19.},
