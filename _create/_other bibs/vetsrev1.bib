	keywords = {Animals, Humans, Risk Factors, Decision Making, *Decision making, computer-assisted, *Decision Support Techniques, *Disease Outbreaks/prevention \& control, *Public health informatics, *Spatial decision support systems, *Zoonoses, Public Health Informatics/*methods, Zoonoses/diagnosis/*epidemiology},
	pages = {38},
	annote = {1476-072xBeard, RachelWentz, ElizabethScotch, MatthewOrcid: 0000-0001-5100-9724F31 LM012176/LM/NLM NIH HHS/United StatesR01 LM012080/LM/NLM NIH HHS/United StatesJournal ArticleResearch Support, N.I.H., ExtramuralSystematic ReviewInt J Health Geogr. 2018 Oct 30;17(1):38. doi: 10.1186/s12942-018-0157-5.},
