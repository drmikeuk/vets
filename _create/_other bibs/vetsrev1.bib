	keywords = {Animals, Cattle, Humans, Incidence, Milk, Middle East, Buffaloes, Brucellosis, Zoonoses/epidemiology, Dairy Products/microbiology, Turkey/epidemiology, *Brucella, *Prevalence, Brucella spp., Brucellosis/*epidemiology, Dairy products, Egypt/epidemiology, Food contamination, interest., Iran/epidemiology, Kuwait/epidemiology, Saudi Arabia/epidemiology, Syria/epidemiology},
