	year = {2020},
	note = {Edition: 2020/07/02},
	keywords = {Animals, Humans, Prevalence, Rats, Risk Factors, Helminth, Review, Research/*trends, City, Developed Countries/statistics \& numerical data, Helminthiasis/epidemiology/parasitology, Public Health/statistics \& numerical data, Rattus norvegicus, Rattus rattus, Rodent Diseases/epidemiology/*parasitology, Urban habitat},
	pages = {2383--2397},
	annote = {1432-1955Gliga, Diana SOrcid: 0000-0002-4484-5393Pisanu, BenoîtWalzer, ChrisOrcid: 0000-0002-0437-5147Desvars-Larrive, AmélieOrcid: 0000-0001-7671-696xFU-282-PDC/Veterinärmedizinische Universität Wien (AT)/Journal ArticleSystematic ReviewParasitol Res. 2020 Aug;119(8):2383-2397. doi: 10.1007/s00436-020-06776-3. Epub 2020 Jun 30.},
