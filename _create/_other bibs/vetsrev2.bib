	keywords = {Animals, Cattle, Sheep, Humans, Prevalence, Swine, China, Food Contamination/*analysis, *Food Microbiology, Monitoring, Poultry/*microbiology, Entropy-based uncertainty analysis, Food sampling, Listeria monocytogenes/*isolation \& purification, Listeriosis, Listeriosis/epidemiology, Meat Products/*microbiology, Microbiological safety},
	pages = {108358},
	annote = {1879-3460Liu, YangtaiSun, WanxiaSun, TianmeiGorris, Leon G MWang, XiangLiu, BaolinDong, QingliJournal ArticleMeta-AnalysisNetherlandsInt J Food Microbiol. 2020 Jan 2;312:108358. doi: 10.1016/j.ijfoodmicro.2019.108358. Epub 2019 Oct 5.},
