	keywords = {Systematic review, Fish consumption, Stroke, Cardiovascular disease, Acute myocardial infarction},
	pages = {713--720},
	annote = {1555-7162Krittanawong, ChayakritIsath, AmeeshHahn, JoshuaWang, ZhenNarasimhan, BharatKaplin, Scott LJneid, HaniVirani, Salim STang, W H WilsonJournal ArticleReviewUnited StatesAm J Med. 2021 Jun;134(6):713-720. doi: 10.1016/j.amjmed.2020.12.017. Epub 2021 Jan 11.},
