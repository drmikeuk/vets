	author = {Esser, H. J. and Mögling, R. and Cleton, N. B. and van der Jeugd, H. and Sprong, H. and Stroo, A. and Koopmans, M. P. G. and de Boer, W. F. and Reusken, Cbem},
	month = may,
	year = {2019},
	note = {Edition: 2019/05/28},
	keywords = {Animals, Humans, Risk Factors, Europe, Rift Valley fever virus, Disease Vectors, West Nile virus, Arbovirus Infections/*epidemiology, Arboviruses/classification/*isolation \& purification, Arthropod Vectors/virology, Crimean-Congo hemorrhagic fever virus, Encephalitis Virus, Japanese/isolation \& purification, Japanese encephalitis virus, Louping-ill virus, Mosquito Vectors/*virology, Rift Valley fever virus/isolation \& purification, Tick-borne encephalitis virus, West Nile virus/isolation \& purification},
