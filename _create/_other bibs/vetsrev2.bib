	keywords = {Animals, Humans, Swine, Risk Factors, Zoonoses, Zoonosis, Africa/epidemiology, Toxoplasmosis, Meta-analysis, Global Health, Temperature, Seroepidemiologic Studies, North America/epidemiology, Europe/epidemiology, Pig, Asia/epidemiology, Toxoplasmosis, Animal/*epidemiology/parasitology, Antibodies, Protozoan/*blood, South America/epidemiology, Toxoplasma/*immunology, Red Meat/*parasitology, Serology, Swine Diseases/*epidemiology/parasitology, Toxoplasmosis/*epidemiology/parasitology},
	pages = {42--52},
