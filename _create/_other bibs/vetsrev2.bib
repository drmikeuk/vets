	keywords = {Animals, *Disease Models, Animal, *animal models, *concussion, *Athletic Injuries, *Brain Concussion, *chronic traumatic encephalopathy, *neurotrauma},
	pages = {1194--1222},
	annote = {1097-4547Hiskens, Matthew IOrcid: 0000-0001-7054-1492Angoa-Pérez, MarianaSchneiders, Anthony GVella, Rebecca KFenning, Andrew SJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesJ Neurosci Res. 2019 Oct;97(10):1194-1222. doi: 10.1002/jnr.24472. Epub 2019 May 28.},
