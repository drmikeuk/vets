	keywords = {Animals, Horses, Humans, Treatment Outcome, Personality, Adult, *equine-assisted psychotherapy, *Equine-Assisted Therapy, *Mental Disorders, *Personality Disorders/therapy, *personality problems, *psychodynamic psychotherapy, *Psychotherapy, Psychodynamic, *systematic review, *traumatization, Psychological Trauma, Psychotherapy},
	annote = {1660-4601Kovács, Gézavan Dijke, AnnemiekEnders-Slegers, Marie-JoseOrcid: 0000-0002-2781-2952Journal ArticleSystematic ReviewInt J Environ Res Public Health. 2020 Aug 5;17(16):5661. doi: 10.3390/ijerph17165661.},
