	keywords = {Animals, Dogs, Treatment Outcome, Arthroscopy/methods/veterinary, Dog Diseases/pathology/surgery, Joint Diseases/pathology/surgery/veterinary, Osteotomy/methods/veterinary},
	pages = {106--109},
	annote = {last updated in refbase Thu, 03 Oct 2013 12:04:50 +0100},
	annote = {PubMed, VIN and CAB searched},
