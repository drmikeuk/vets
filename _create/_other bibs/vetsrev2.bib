@article{ghanad_systematic_2021,
	title = {A {Systematic} {Review} of {Nonautologous} {Graft} {Materials} {Used} in {Human} {Tympanoplasty}},
	volume = {131},
	issn = {0023-852x},
	doi = {10.1002/lary.28914},
	abstract = {OBJECTIVES: Nonautologous graft materials may solve several dilemmas in tympanoplasty by obviating the need for graft harvest, facilitating consistent wound healing, and permitting graft placement in the clinical setting. Prior studies of nonautologous grafts in humans have shown variable outcomes. In this systematic review, we aim to 1) summarize clinical outcomes and 2) discuss limitations in the literature regarding nonautologous grafts for tympanoplasty in humans. METHODS: A literature review was performed using the Preferred Reporting Items for Systematic Reviews and Meta-Analyses (PRISMA) recommendations. The study size, etiology and duration of perforation, type of nonautologous graft, and postoperative closure rate were assessed. RESULTS: The PRISMA approach yielded 61 articles, including 3,247 ears that met inclusion criteria. Studies evaluated nonautologous grafts including paper patch, gelatin sponge, growth factors, porcine small-intestinal submucosa, among others. Traumatic perforations (62.3\%) were most commonly studied, whereas postinfectious perforations (31.9\%) and other etiologies (5.8\%) comprised a minority of cases. Acute perforations of {\textless}8 weeks duration constituted just over half of all treated ears. Overall closure rate was 82.1\%, with significantly higher closure rates in acute (89.9\%) versus chronic perforations (64.9\%, P {\textless} .01), regardless of material. A median postoperative air-bone gap of 5.6 dB was found in the 23\% of studies reporting this metric. CONCLUSIONS: The majority of publications reviewing nonautologous materials in tympanoplasty evaluate acute or traumatic perforations, and few rigorously report hearing outcomes. Given available data, porcine submucosa and basic fibroblast growth factor may hold promise for chronic perforation closure. Future studies should report closure rates and hearing outcomes in perforations {\textgreater}8 weeks duration. Laryngoscope, 131:392-400, 2021.},
	language = {eng},
	number = {2},
	journal = {Laryngoscope},
	author = {Ghanad, I. and Polanik, M. D. and Trakimas, D. R. and Knoll, R. M. and Castillo-Bustamante, M. and Black, N. L. and Kozin, E. D. and Remenschneider, A. K.},
	month = feb,
	year = {2021},
	note = {Edition: 2020/11/12},
	keywords = {Animals, Humans, Female, Male, Treatment Outcome, Swine, Child, Adolescent, Adult, Postoperative Period, Hearing, *Bioprosthesis, *Heterografts, *synthetic graft, *tympanic membrane, *Tympanoplasty, Prosthesis Design, Transplants/*transplantation, Tympanic Membrane Perforation/physiopathology/*surgery, Tympanic Membrane/*transplantation, Tympanoplasty/*methods},
	pages = {392--400},
