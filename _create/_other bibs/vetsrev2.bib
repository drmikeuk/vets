	pages = {276},
	annote = {1471-244xKlimova, BlankaToman, JosefKuca, KamilOrcid: 0000-0001-9664-1109Journal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewBMC Psychiatry. 2019 Sep 6;19(1):276. doi: 10.1186/s12888-019-2245-x.},
