	author = {Gee, N. R. and Mueller, M. K.},
	year = {2019},
	keywords = {reviews, systematic reviews, interactions, research, evaluation, guidelines, health, intervention, older adults, aging, studies, pet animals, pets, Pets and Companion Animals [LL070], recommendations, man, anthrozoology, quality, ageing, exercise, Social Psychology and Social Anthropology [UU485], age, aged, elderly, elderly people, Human Physiology and Biochemistry [VV050], senior citizens},
