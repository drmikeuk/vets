	journal = {Hypertension},
	author = {Kooiman, J. and Terstappen, F. and van Wagensveld, L. and Franx, A. and Wever, K. E. and Roseboom, T. J. and Joles, J. A. and Gremmels, H. and Lely, A. T.},
