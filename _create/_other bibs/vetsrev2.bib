	pages = {956--965},
	annote = {1950-6007Hoxha, MalvinaLewis-Mikhael, Anne-MaryBueno-Cavanillas, AuroraJournal ArticleReviewFranceBiomed Pharmacother. 2018 Oct;106:956-965. doi: 10.1016/j.biopha.2018.07.033. Epub 2018 Jul 12.},
