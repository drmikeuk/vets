	keywords = {Animals, Cattle, Goats, Sheep, Humans, Food Microbiology, Meat Products/microbiology, Dairying, Meat/microbiology, Food Contamination/analysis/prevention \& control, Cattle Diseases/epidemiology, Crohn Disease/microbiology, Food Handling, Milk/microbiology, Mycobacterium avium subsp. paratuberculosis, Paratuberculosis/epidemiology/microbiology/prevention \& control, Sterilization/methods},
	pages = {1061--1071},
	annote = {last updated in refbase Wed, 02 Oct 2013 10:54:41 +0100},
	annote = {PubMed, CAB Direct (CAB Abstracts?) and Science Direct searched},
	annote = {UoN @ douglas.grindlay @ 659},
