	note = {Edition: 2020/05/10},
	keywords = {Animals, Humans, Female, Risk Factors, Prospective Studies, Milk, Adult, *Diet, Dairy, Bone, *Dairy Products, *Hip Fractures/epidemiology/etiology/prevention \& control, Cheese, Fracture, Yogurt},
	pages = {1411--1425},
	annote = {1433-2965Hidayat, KOrcid: 0000-0002-4095-6969Du, XShi, B-MQin, L-Q2017YFC1310700/National Key R\&D Program of China/2017YFC1310701/National Key R\&D Program of China/SYS201741/Suzhou Science and Technology Bureau/Journal ArticleMeta-AnalysisSystematic ReviewEnglandOsteoporos Int. 2020 Aug;31(8):1411-1425. doi: 10.1007/s00198-020-05383-3. Epub 2020 May 7.},
