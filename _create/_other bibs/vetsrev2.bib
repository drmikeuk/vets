	note = {Edition: 2018/02/07},
	keywords = {Animals, Humans, Rats, Swine, *Disease Models, Animal, Publication Bias, Reproducibility of Results, *Heart Arrest, *Random Allocation, Research Design/*standards},
	pages = {16--21},
