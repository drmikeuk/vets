	pages = {717--729},
	annote = {1590-3729Karimi, GolgisHeidari, ZahraFirouzi, SomayyehHaghighatdoost, FahimehJournal ArticleMeta-AnalysisSystematic ReviewNetherlandsNutr Metab Cardiovasc Dis. 2020 May 7;30(5):717-729. doi: 10.1016/j.numecd.2020.02.001. Epub 2020 Feb 12.},
