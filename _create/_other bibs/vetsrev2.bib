	keywords = {Animals, Humans, Disease Models, Animal, Regeneration, *Stem cells, Tissue Scaffolds/*chemistry, Tissue Engineering/*methods, *Biomaterials, *Cartilage regeneration, *Fibroins, *Scaffold, *Silk fibroin, Biocompatible Materials/chemistry, Bombyx/metabolism, Cartilage/*physiology},
	pages = {1583--1594},
	annote = {1522-9653Fazal, NLatief, NJournal ArticleSystematic ReviewEnglandOsteoarthritis Cartilage. 2018 Dec;26(12):1583-1594. doi: 10.1016/j.joca.2018.07.009. Epub 2018 Jul 29.},
