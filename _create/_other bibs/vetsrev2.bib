	pages = {89--93},
	annote = {1532-2661Ludwick, KristenClymer, Jeffrey WComparative StudyJournal ArticleMeta-AnalysisEnglandRes Vet Sci. 2019 Aug;125:89-93. doi: 10.1016/j.rvsc.2019.05.013. Epub 2019 May 22.},
