	pages = {230},
	annote = {2042-7670Hertzsch, RobertOrcid: 0000-0002-7297-4790Richter, AngelikaJournal ArticleSystematic ReviewEnglandVet Rec. 2019 Aug 24;185(8):230. doi: 10.1136/vr.105404. Epub 2019 Aug 13.},
}

