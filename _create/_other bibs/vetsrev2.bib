	keywords = {Animals, Humans, Prevalence, Female, Male, Child, Incidence, Aged, Adolescent, Adult, Middle Aged, Young Adult, Child, Preschool, Aged, 80 and over, Preoperative Period, Water/parasitology, *Spirometra, Central Nervous System, Immunocompromised Patient, Korea, Republic of Korea/epidemiology, Sparganosis, Sparganosis/*diagnosis/*epidemiology, Sparganum, Subcutaneous Tissue},
	pages = {e273},
