	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba7&AN=20203148656 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=In+vitro+and+in+vivo+antitumor+activity+of+vitamin+D3+in+malignant+gliomas%3A+a+systematic+review.&title=International+Journal+of+Cancer+Management&issn=2538-4422&date=2020&volume=13&issue=2&spage=&aulast=Hajimohammadebrahim-Ketabforoush%2C+M.&isbn=&__char_set=utf8},
