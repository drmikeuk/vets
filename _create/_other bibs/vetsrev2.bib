	keywords = {Animals, Humans, Mice, Gastrointestinal Microbiome, Culicidae/parasitology, *Host-Parasite Interactions, *Microbiota, Malaria/*prevention \& control/transmission, Plasmodium, Skin/*microbiology},
	pages = {1831--1839},
	annote = {1537-6591Ippolito, Matthew MDenny, Joshua ELangelier, CharlesSears, Cynthia LSchmidt, Nathan WK23 HL138461/HL/NHLBI NIH HHS/United StatesR01 AI123486/AI/NIAID NIH HHS/United StatesT32 GM066691/GM/NIGMS NIH HHS/United StatesJournal ArticleResearch Support, N.I.H., ExtramuralResearch Support, Non-U.S. Gov'tSystematic ReviewClin Infect Dis. 2018 Nov 28;67(12):1831-1839. doi: 10.1093/cid/ciy374.},
