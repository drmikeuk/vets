	keywords = {Cattle, Goats, Sheep, Paratuberculosis},
	annote = {last updated in refbase Wed, 11 Feb 2015 14:48:14 +0000},
	annote = {Scopus, PubMed, Redalyc, Virtual Health Library and International Association for Paratuberculosis Website searched},
