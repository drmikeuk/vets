@article{lefebvre_therapeutic_2012,
	title = {Therapeutic efficiency of antibiotics and prostaglandin {F2alpha} in postpartum dairy cows with clinical endometritis: an evidence-based evaluation},
	volume = {28},
	issn = {1558-4240},
	url = {http://dx.doi.org/10.1016/j.cvfa.2012.01.002},
	doi = {10.1016/j.cvfa.2012.01.002},
	abstract = {The occurrence of vaginal discharge in postpartum dairy cows is generally diagnosed as clinical endometritis. This uterine condition is associated with reduced fertility and economic loss for the dairy industry. Therapeutic approaches include the systemic or intrauterine application of antibiotics or the injection of prostaglandin F2alpha and analogues to cause luteolysis and uterine contractions to evacuate the infected content. The treatment of clinical endometritis remains a subject of considerable controversy in the literature. Better understanding of the reproductive biology of normal versus abnormal uterine involution and immune mechanisms will allow more efficient diagnostic methods and a more efficient therapeutic approach.},
	language = {English},
