	keywords = {Animals, Humans, Swine, Risk Factors, Pigs, Influenza, Asia, Human/epidemiology/prevention \& control/transmission, Influenza A virus/classification/isolation \& purification, Sentinel Surveillance, Southeastern/epidemiology, Zoonoses/epidemiology/etiology},
