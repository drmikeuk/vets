	keywords = {Animals, Humans, Prevalence, *meta-analysis, Insect Vectors/parasitology, Psychodidae/parasitology, Iran/epidemiology, Disease Reservoirs/parasitology, *leishmaniasis, *Iran, * Lishmania major, *Leishmania major, *sand fly, Leishmaniasis, Cutaneous/transmission, Phlebotomus/*parasitology},
	pages = {26--36},
