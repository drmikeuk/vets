	keywords = {meta-analysis, prognosis, osteosarcoma, mice, microRNA-34a},
	pages = {4179--4186},
	annote = {1837-9664Wang, WenyiHu, ShaopuChang, JunliRuan, HongfengZhi, WenlanWang, XiaoboShi, QiWang, YongjunYang, YanpingJournal ArticleJ Cancer. 2018 Oct 18;9(22):4179-4186. doi: 10.7150/jca.27483. eCollection 2018.},
}

