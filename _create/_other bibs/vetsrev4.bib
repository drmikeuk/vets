	pages = {217--228},
	annote = {1475-2654Winder, C BOrcid: 0000-0002-7314-3657Sargeant, J MOrcid: 0000-0003-2420-1766Kelton, D FLeblanc, S JDuffield, T FGlanville, JWood, HChurchill, K JDunn, JBergevin, M DDawkins, KMeadows, SO'Connor, A MOrcid: 0000-0003-0604-7822Journal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewEnglandAnim Health Res Rev. 2019 Dec;20(2):217-228. doi: 10.1017/S1466252319000306.},
