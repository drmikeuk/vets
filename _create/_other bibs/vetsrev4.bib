	note = {Edition: 2020/02/23},
	keywords = {Animals, *meta-analysis, *systematic review, *Systematic Reviews as Topic, *Livestock, *Animal health, *animal performance, *Meta-Analysis as Topic, *on-farm food safety, *Research Report, *scoping review},
	pages = {116--127},
