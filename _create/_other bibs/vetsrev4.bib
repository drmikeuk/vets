	pages = {12--19},
	annote = {1476-5829Sayag, DOrcid: 0000-0001-5084-8892Fournel-Fleury, CPonce, FJournal ArticleReviewSystematic ReviewEnglandVet Comp Oncol. 2018 Mar;16(1):12-19. doi: 10.1111/vco.12320. Epub 2017 May 19.},
