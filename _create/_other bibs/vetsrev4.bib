@article{sandrini_association_2018,
	title = {Association between obesity and circulating brain-derived neurotrophic factor ({BDNF}) levels: systematic review of literature and meta-analysis},
	volume = {19},
	issn = {1661-6596},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20183326113 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Association+between+obesity+and+circulating+brain-derived+neurotrophic+factor+%28BDNF%29+levels%3A+systematic+review+of+literature+and+meta-analysis.&title=International+Journal+of+Molecular+Sciences&issn=1661-6596&date=2018&volume=19&issue=8&spage=2281&aulast=Sandrini%2C+L.&isbn=&__char_set=utf8},
	doi = {10.3390/ijms19082281},
	abstract = {Reduction in brain-derived neurotrophic factor (BDNF) expression in the brain as well as mutations in BDNF gene and/or of its receptor are associated to obesity in both human and animal models. However, the association between circulating levels of BDNF and obesity is still not defined. To answer this question, we performed a meta-analysis carrying out a systematic search in electronic databases. Ten studies (307 obese patients and 236 controls) were included in the analysis. Our data show that obese patients have levels of BDNF similar to those of controls (SMD: 0.01, 95\% CI: -0.28, 0.30, p=0.94). The lack of difference was further confirmed both in studies in which BDNF levels were assessed in serum (MD: -0.93 ng/mL, 95\% CI: -3.34, 1.48, p=0.45) and in plasma (MD: 0.15 ng/mL, 95\% CI: -0.09, 0.39, p=0.23). Data evaluation has shown that some bias might affect BDNF measurements (e.g., subject recruitment, procedures of sampling, handling, and storage), leading to a difficult interpretation of the results. Standardization of the procedures is still needed to reach strong, affordable, and reliable conclusions.},
	language = {English},
	number = {8},
	journal = {International Journal of Molecular Sciences},
	author = {Sandrini, L. and Minno, A. di and Amadio, P. and Ieraci, A. and Tremoli, E. and Barbieri, S. S.},
	year = {2018},
	keywords = {models, systematic reviews, meta-analysis, research, databases, data analysis, evaluation, blood plasma, animal models, relationships, Information and Documentation [CC300], studies, data banks, standardization, plasma (blood), man, Animal Models of Human Diseases [VV400], obesity, Animal Models of Human Nutrition [VV140], fatness, mutations, brain, cerebrum},
	annote = {Using Smart Source Parsing2281. 70 ref},
}

