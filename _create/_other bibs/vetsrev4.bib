	journal = {Crit Care},
	author = {Smit, B. and Smulders, Y. M. and Eringa, E. C. and Oudemans-van Straaten, H. M. and Girbes, A. R. J. and Wever, K. E. and Hooijmans, C. R. and Spoelstra-de Man, A. M. E.},
	month = aug,
