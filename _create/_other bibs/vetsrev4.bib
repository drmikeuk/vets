	keywords = {Animals, Horses, Risk Factors, Research Design/standards, Foot Diseases/epidemiology/pathology/physiopathology/veterinary, Hoof and Claw/pathology/physiopathology, Horse Diseases/epidemiology/pathology/physiopathology},
