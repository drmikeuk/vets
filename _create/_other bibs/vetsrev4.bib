	pages = {138--154},
	annote = {1099-1573Shi, KaiyunWen, JianxiaZeng, JinhaoGuo, YaoguangHu, JikeLi, CongZhao, YanlingMa, XiaoOrcid: 0000-0002-0261-0369National Natural Science Foundation of China: 81874365/Sichuan Science and Technology Program: 2019YJ0492/Chengdu University of TCM Fund Grant: QNXZ2018025/Journal ArticleMeta-AnalysisSystematic ReviewEnglandPhytother Res. 2021 Jan;35(1):138-154. doi: 10.1002/ptr.6806. Epub 2020 Sep 25.},
}

