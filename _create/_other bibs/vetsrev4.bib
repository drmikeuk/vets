@article{yuan_timing_2020,
	title = {Timing of {Cow}'s {Milk} or {Cow}'s {Milk} {Formula} {Introduction} to the {Infant} {Diet} and {Atopic} {Risk} in {Children}: a {Systematic} {Review} and {Meta}-analysis},
	volume = {59},
	issn = {1080-0549},
	doi = {10.1007/s12016-019-08770-9},
	abstract = {Infant feeding is an important early-life exposure that may influence the development of atopic disease. The optimal timing of introduction of food allergens, including cow's milk (CM), is not known. This study aims to systematically review the evidence describing the effects of timing of CM or cow's milk formula (CMF) introduction to the infant diet on the development of atopic diseases during childhood. Pubmed, Embase, CINAHL, Cochrane CENTRAL, and CNKI were searched through May 30, 2019. Study screening and data extraction by two reviewers followed the PRISMA statement. Data were extracted independently in duplicate, and meta-analyses were performed by pooling unadjusted and adjusted odds ratio (OR) separately. Heterogeneity was explored using I(2) and publication bias by funnel plots and Begg's tests. In total, 45 studies from 20 countries were included. Meta-analyses using adjusted data showed that no associations were observed between early introduction of CM or CMF and the risk of asthma ({\textless}â€‰4 vs. â‰¥â€‰4Â months: OR 1.16, 95\% confidence interval (CI) 0.89, 1.51), wheeze ({\textless}â€‰6 vs. â‰¥â€‰6Â months: OR 1.15, 95\% CI 0.85, 1.56), and eczema or atopic dermatitis ({\textless}â€‰6 vs. â‰¥â€‰6Â months: OR 0.96, 95\% CI 0.65, 1.41). Overall, quite little high-quality evidence was identified to allow for definitive conclusions on the association between early CM or CMF introduction and risk of allergic diseases. Our meta-analysis on this topic highlights the specific gaps in information for public recommendations regarding CM or CMF feeding practice in an early stage of life, particularly before 3Â months of age.},
	language = {eng},
	number = {1},
	journal = {Clin Rev Allergy Immunol},
	author = {Yuan, M. and Tan, M. and Moore, D. and Shen, S. and Qiu, X. and Thomas, G. N. and Cheng, K.},
	month = aug,
	year = {2020},
	note = {Edition: 2019/11/27},
	keywords = {Animals, Cattle, Humans, Child, Milk, Meta-analysis, Systematic review, Infant, Infant Formula, Risk, Cowâ€™s milk, Allergy, Allergens/immunology, Asthma/*immunology, Diet Therapy/*methods, Hypersensitivity, Immediate/*immunology, Milk Hypersensitivity/*immunology, Milk Proteins/immunology, Timing of introduction},
	pages = {46--60},
	annote = {1559-0267Yuan, MingyangTan, MiaoyuMoore, DavidShen, SongyingQiu, XiuThomas, G NeilCheng, Kar201807010086/Guangzhou Science and Technology Bureau/81673181/Innovative Research Group Project of the National Natural Science Foundation of China/81703244/Innovative Research Group Project of the National Natural Science Foundation of China/Journal ArticleReviewSystematic ReviewUnited StatesClin Rev Allergy Immunol. 2020 Aug;59(1):46-60. doi: 10.1007/s12016-019-08770-9.},
