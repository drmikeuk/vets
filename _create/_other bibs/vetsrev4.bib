@article{sjofjan_effects_2021,
	title = {The effects of probiotics on the performance, egg quality and blood parameters of laying hens: a meta-analysis},
	volume = {30},
	issn = {1230-1388},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba7&AN=20210200800 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=The+effects+of+probiotics+on+the+performance%2C+egg+quality+and+blood+parameters+of+laying+hens%3A+a+meta-analysis.&title=Journal+of+Animal+and+Feed+Sciences&issn=1230-1388&date=2021&volume=30&issue=1&spage=11&aulast=Sjofjan%2C+O.&isbn=&__char_set=utf8},
	doi = {10.22358/jafs/133432/2021},
	abstract = {A meta-analysis was conducted to determine the effects of probiotics on the performance, egg quality and blood parameters in laying hens. A database was designed based on published papers reporting the use of probiotics in laying hens. Articles were rigorously selected according to the Systematic Review Centre for Laboratory Animal Experimentation (SYRCLE) protocols. The final database consisted of 47 in vivo studies with 190 treatment units. The statistical meta-analysis was performed according to the linear mixed models by using R software version 3.6.3. It was shown that dietary addition of probiotics linearly increased (P {\textless} 0.001) egg production and concomitantly decreased (P {\textless} 0.01) feed egg ratio (FER) with a linear pattern. Egg mass and feed intake were not associated with the probiotic treatment. Concerning egg quality parameters, probiotics did not affect egg weight but increased eggshell thickness (P {\textless} 0.001), eggshell weight (P {\textless} 0.01) and yolk colour (P {\textless} 0.01). Probiotics reduced (P {\textless} 0.05) cholesterol and low-density lipoprotein cholesterol while elevated (P {\textless} 0.05) high-density lipoprotein cholesterol blood concentrations. In conclusion, poultry products with health-promoting properties can be obtained with the use of probiotics which positively affect production performance, egg quality and blood metabolites parameters in lying hens.},
	language = {English},
	number = {1},
	journal = {Journal of Animal and Feed Sciences},
	author = {Sjofjan, O. and Adli, D. N. and Sholikin, M. M. and Jayanegara, A. and Irawan, A.},
	year = {2021},
	keywords = {feed intake, poultry, models, systematic reviews, meta-analysis, effects, research, databases, data analysis, animal feeding, diets, feeds, feed additives, probiotics, blood chemistry, yields, performance, laboratory animals, metabolites, cholesterol, computer software, birds, chickens, domesticated birds, Egg Producing Animals [LL130], egg weight, Eggs and Egg Products [QQ040], Food Composition and Quality [QQ500], fowls, hens, studies, data banks, Animal Nutrition (Production Responses) [LL520], feeding stuffs, Feed Additives [RR130], egg production, eggs, Animal Nutrition (Physiology) [LL510], blood, properties, hen feeding, computer programs, food quality, high density lipoprotein, egg quality, egg shell thickness, blood picture, egg yolk color, egg yolk colour, lipoproteins, poultry products},
