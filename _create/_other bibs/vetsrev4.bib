	keywords = {Animals, Cattle, Female, Dairying, Lactation/drug effects/physiology, Animal/drug therapy/pathology, Anthelmintics/pharmacology/therapeutic use, Cattle Diseases/drug therapy/pathology, Helminthiasis, Milk/drug effects/physiology},
	pages = {237--256},
	annote = {CAB Abstracts and MEDLINE searched},
