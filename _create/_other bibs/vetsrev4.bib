	note = {Edition: 2019/06/19},
	keywords = {Animals, Horses, Sheep, Dogs, Humans, Mice, Rats, Rabbits, Body Weight, Meta-analysis, Disease Models, Animal, Haplorhini, Cancer, Guinea Pigs, Cricetinae, Dose-Response Relationship, Drug, Tumor Burden, Neoplasms/*drug therapy/metabolism, Berberine, *Phytotherapy, Angiogenesis Inhibitors/administration \& dosage/adverse effects/*therapeutic use, Antineoplastic Agents/administration \& dosage/adverse effects/*therapeutic use, Berberine/administration \& dosage/adverse effects/*therapeutic use, Berberis/chemistry, Experimental animals, Plant Extracts/therapeutic use},
	pages = {589},
