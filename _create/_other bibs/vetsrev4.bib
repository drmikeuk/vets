	month = apr,
	year = {2021},
	note = {Edition: 2020/08/08},
	keywords = {Animals, Anti-Bacterial Agents/therapeutic use, Titanium, Staphylococcus aureus, *Biofilm, *Coating, *Infection prevention, *Methicillin-Resistant Staphylococcus aureus, *Prophylaxis, *Staphylococcal Infections/drug therapy/prevention \& control, *Staphylococcus, *Titanium, Prostheses and Implants},
	pages = {821--835},
	annote = {1432-5195Tsikopoulos, KonstantinosOrcid: 0000-0001-5923-4782Sidiropoulos, KonstantinosKitridis, DimitriosHassan, AnasDrago, LorenzoMavrogenis, AndreasMcBride, DonaldJournal ArticleMeta-AnalysisSystematic ReviewGermanyInt Orthop. 2021 Apr;45(4):821-835. doi: 10.1007/s00264-020-04660-4. Epub 2020 Aug 6.},
