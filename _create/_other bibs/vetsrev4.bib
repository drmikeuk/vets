	keywords = {Animals, Humans, Anti-Bacterial Agents/*therapeutic use, *meta-analysis, *systematic review, *Livestock, *amstar 2, *animal health, *non-antibiotic management, *preventative antibiotics, *quality assessment, Bacterial Infections/microbiology/*veterinary},
	pages = {305--318},
	annote = {1475-2654Vriezen, RachaelSargeant, Jan MOrcid: 0000-0003-2420-1766Vriezen, EllenWinder, Charlotte BOrcid: 0000-0002-7314-3657O'Connor, Annette MOrcid: 0000-0003-0604-7822Journal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewEnglandAnim Health Res Rev. 2019 Dec;20(2):305-318. doi: 10.1017/S146625231900029X.},
