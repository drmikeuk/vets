	number = {2},
	journal = {Anim Health Res Rev},
	author = {Sargeant, J. M. and Bergevin, M. D. and Churchill, K. and Dawkins, K. and Deb, B. and Dunn, J. and Logue, C. M. and Novy, A. and O'Connor, A. M. and Reist, M. and Winder, C. B.},
