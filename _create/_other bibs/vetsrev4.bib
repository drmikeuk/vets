	pages = {99--126},
	annote = {1559-0720White, Pandora EVincent, John BJournal ArticleSystematic ReviewUnited StatesBiol Trace Elem Res. 2019 Mar;188(1):99-126. doi: 10.1007/s12011-018-1575-8. Epub 2018 Nov 14.},
