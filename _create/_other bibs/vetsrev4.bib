	pages = {e00173},
	annote = {2405-6731Soosaraei, MasoudHaghi, Mousa MotavalliEtemadifar, FariborzFakhar, MahdiTeshnizi, Saeed HosseiniAsfaram, ShabnamEsboei, Bahman RahimiJournal ArticleReviewParasite Epidemiol Control. 2020 Aug 13;11:e00173. doi: 10.1016/j.parepi.2020.e00173. eCollection 2020 Nov.},
