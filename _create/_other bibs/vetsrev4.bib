	keywords = {Animals, Animal models, systematic review, pain, Disease Models, Animal, Pain Management/*methods, dorsal root ganglion, Electric Stimulation Therapy/*methods, Ganglia, Spinal/*physiology, Neuralgia/physiopathology/*therapy, neurostimulation},
