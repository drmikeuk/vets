	month = aug,
	year = {2020},
	note = {Edition: 2020/08/18},
	keywords = {cattle, Meta-analysis, leptin, polymorphisms, production traits, Uasms2},
	pages = {1--10},
	annote = {1532-2378Wang, LiyunRaza, Sayed Haidar AbbasOrcid: 0000-0002-0961-1911Gui, LinshengLi, ShijunLiu, XuchunYang, XinranWang, SihuZan, LinsenZhao, ChunpingJournal ArticleEnglandAnim Biotechnol. 2020 Aug 17:1-10. doi: 10.1080/10495398.2020.1805327.},
}

@article{wang_conversion_2021,
