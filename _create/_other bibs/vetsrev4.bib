	pages = {563--573},
	annote = {1681-7168Wahid, BrairaAltaf, SabaNaeem, NabihaIlyas, NimraIdrees, MuhammadSystematic ReviewPakistanJ Coll Physicians Surg Pak. 2019 Jun;29(6):563-573. doi: 10.29271/jcpsp.2019.06.563.},
