	keywords = {Animals, Fish, Liver, Hepatotoxicity, Fishes/*metabolism, Endocrine, Toxicoloy, Histopathology, Endocrine disruption, Androgens/*toxicity, Data credibility, Endocrine Disruptors/*toxicity, Estrogens/*toxicity, Liver/*drug effects/*pathology, Water Pollutants, Chemical/toxicity},
	pages = {60--78},
