	number = {4},
	journal = {Journal of Morphological Sciences},
	author = {Xavier, J. E. and Antonio, E. A. and Oliveira, L. dos S. and Souza, S. L. de and Medeiros, J. P. de and Lima, L. C. and Fraga, C. A. de C. and Miguel, R. D. da S.},
