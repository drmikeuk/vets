	keywords = {Animals, Humans, Child, Animal models, Models, Animal, Brain/*pathology, Biomechanical Phenomena, Child Abuse, Closed head injuries, Forensic pathology, Head Injuries, Closed/*pathology, Shaken Baby Syndrome/*pathology},
	pages = {408--422},
	annote = {1556-2891Vester, Marloes E MOrcid: 0000-0002-6507-3441Bilo, Rob A CLoeve, Arjo Jvan Rijn, Rick Rvan Zandwijk, Jan PeterJournal ArticleSystematic ReviewForensic Sci Med Pathol. 2019 Sep;15(3):408-422. doi: 10.1007/s12024-019-0082-3. Epub 2019 Mar 4.},
