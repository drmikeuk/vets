	keywords = {Animals, Humans, Mice, Systematic review, Models, Animal, Vitamin D, Diet/*methods, Gastrointestinal Microbiome/*physiology, Gut microbiota, Microbiome, Vitamin D/*blood, Vitamins/*blood},
	pages = {2895--2910},
	annote = {1436-6215Waterhouse, MaryHope, BronwynKrause, LutzMorrison, MarkProtani, Melinda MZakrzewski, MarthaNeale, Rachel EJournal ArticleSystematic ReviewGermanyEur J Nutr. 2019 Oct;58(7):2895-2910. doi: 10.1007/s00394-018-1842-7. Epub 2018 Oct 15.},
