	keywords = {Animals, Aquaculture, Water Pollutants, Chemical/*toxicity, Antiparasitic Agents/*toxicity, Azamethiphos, Bivalvia/*drug effects, Crustacea/*drug effects, Crustaceans and non-target bivalves, Cypermethrin, Deltamethrin, Hydrogen peroxide, Salmon farming},
	pages = {1124--1136},
