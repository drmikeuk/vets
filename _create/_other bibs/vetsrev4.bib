	keywords = {beef, food contamination, food safety, Salmonella, microbial contamination, strains, efficacy, antibiotics, food contaminants, Meat Produce [QQ030], antibacterial agents, drug resistance, Pesticide and Drug Resistance [HH410], Escherichia coli, meat products, Food Contamination, Residues and Toxicology [QQ200], E coli, Prion, Viral, Bacterial and Fungal Pathogens of Humans [VV210], drug susceptibility},
	pages = {3--23},
