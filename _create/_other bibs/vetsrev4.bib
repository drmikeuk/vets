	pages = {145--155},
	annote = {Food Science and Technology Abstracts, CAB Direct (CAB Abstracts?), AGRICOLA, Biological Abstracts, Biological Sciences, Web of Science, Digital Dissertations, Scholars Portal, Faculty of 1000, Canadian Research Index, Cochrane Library, CISTI Source, CINAHL, MEDLINE (CSA), Science Citation Index, PubMed, Ingenta, EMBASE, Food Science and Technology Abstracts, CAB Health, Current Contents and MEDLINE searched},
