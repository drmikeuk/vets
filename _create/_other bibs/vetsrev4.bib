	pages = {e20713},
	annote = {1536-5964Yu, XinZhang, YuanLiu, ShuangMu, YuShang, FengjiaZhang, NanOrcid: 0000-0002-2375-3051Journal ArticleMeta-AnalysisSystematic ReviewMedicine (Baltimore). 2020 Jul 2;99(27):e20713. doi: 10.1097/MD.0000000000020713.},
