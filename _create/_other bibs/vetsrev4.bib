	pages = {439--457},
	annote = {1863-2378Willemsen, AngelaOrcid: 0000-0003-2198-9500Cobbold, RowlandOrcid: 0000-0002-5560-4237Gibson, JustineOrcid: 0000-0003-2202-8082Wilks, KathrynLawler, SheleighOrcid: 0000-0002-5771-0551Reid, SimonOrcid: 0000-0002-6103-4429Journal ArticleSystematic ReviewGermanyZoonoses Public Health. 2019 Aug;66(5):439-457. doi: 10.1111/zph.12589. Epub 2019 May 31.},
