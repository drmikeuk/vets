	pages = {33--47},
	annote = {1879-1506Scheidegger, RolfKoletsi, DespinaEliades, TheodoreJournal ArticleMeta-AnalysisReviewSystematic ReviewEnglandArch Oral Biol. 2018 Oct;94:33-47. doi: 10.1016/j.archoralbio.2018.06.016. Epub 2018 Jun 19.},
