	keywords = {Animals, Humans, Randomized Controlled Trials as Topic, animal experimentation, Infant, Infant, Newborn, Child, Preschool, anesthesia, neurotoxicity, Cognition/drug effects, newborn, Hypnotics and Sedatives/*pharmacology, sedation, Adrenergic alpha-2 Receptor Agonists/*pharmacology, brain/growth \& development, child development/drug effects, dexmedetomidine, Dexmedetomidine/*pharmacology, general, Mental Status and Dementia Tests, Neurotoxicity Syndromes/etiology},
	pages = {125--136},
