	keywords = {Animals, Humans, Rats, Rabbits, Treatment Outcome, Aged, Adolescent, Adult, Middle Aged, Young Adult, Drug Evaluation, Preclinical, Recovery of Function, Clinical Trials as Topic, Drug Evaluation, Biological Availability, *neuroprotection, *spinal cord injury, *SCI = spinal cord injury, Glutamic Acid/metabolism, *review, *AIS = American Spinal Injury Association Impairment Scale, *ALS = amyotrophic lateral sclerosis, *excitotoxicity, *riluzole, Excitatory Amino Acid Antagonists/pharmacokinetics/therapeutic use, Neuroprotective Agents/pharmacokinetics/*therapeutic use, Riluzole/pharmacokinetics/*therapeutic use, Spinal Cord Injuries/complications/*drug therapy, Trauma Severity Indices},
	pages = {E6},
