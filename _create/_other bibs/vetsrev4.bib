	author = {Silva, C. E. de L. da and Oyama, J. and Ferreira, F. B. P. and Lalucci-Silva, M. P. de P. and Lordani, T. V. A. and Silva, R. C. de L. da and Monich, M. de S. T. and Teixeira, J. J. V. and Lonardoni, M. V. C.},
