	keywords = {Animals, Animals, Wild, *Parasitic Diseases, *Host-Parasite Interactions, virulence, *Parasites, Fitness, host-parasite interaction, infectious disease ecology, phylogenetic meta-analysis, publication bias, susceptibility},
	pages = {1869--1884},
	annote = {1461-0248SÃ¡nchez, Cecilia ABecker, Daniel JTeitelbaum, Claire SBarriga, PaolaBrown, Leone MMajewska, Ania AHall, Richard JAltizer, SoniaNSF DEB-1518611/National Science Foundation (NSF)/NSF DEB-1601052/National Science Foundation (NSF)/NSF DBI-1402287/National Science Foundation (NSF)/ARCS Foundation Awards/Wormsloe Foundation/UGA Presidential Fellowship/Journal ArticleMeta-AnalysisReviewEnglandEcol Lett. 2018 Dec;21(12):1869-1884. doi: 10.1111/ele.13160. Epub 2018 Oct 4.},
