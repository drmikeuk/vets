	note = {Edition: 2018/01/10},
	keywords = {Animals, Female, Male, Reproduction, Species Specificity, Diet, Bird Diseases/parasitology, Host-Parasite Interactions, *Color, *Pigmentation, Carotenoids/*metabolism, Disease Resistance, Feathers/chemistry/*metabolism, Songbirds/classification/*metabolism/parasitology},
	pages = {73},
	annote = {2041-1723Weaver, Ryan JOrcid: 0000-0002-6160-4735Santos, Eduardo S ATucker, Anna MWilson, Alan EHill, Geoffrey EJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tNat Commun. 2018 Jan 8;9(1):73. doi: 10.1038/s41467-017-02649-z.},
