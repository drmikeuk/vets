	keywords = {Sepsis, Cognitive impairment, Clp, Lps, Memory impairment, Sae, Sepsis-associated encephalopathy},
	pages = {386--404},
	annote = {1873-7528Savi, Felipe Figueredode Oliveira, Alexandrede Medeiros, Gabriela FerreiraBozza, Fernando AugustoMichels, MoniqueSharshar, TarekDal-Pizzol, FelipeRitter, CristianeJournal ArticleReviewUnited StatesNeurosci Biobehav Rev. 2020 Dec 9;124:386-404. doi: 10.1016/j.neubiorev.2020.12.005.},
	annote = {1873-7528Savi, Felipe Figueredode Oliveira, Alexandrede Medeiros, Gabriela FerreiraBozza, Fernando AugustoMichels, MoniqueSharshar, TarekDal-Pizzol, FelipeRitter, CristianeJournal ArticleReviewUnited StatesNeurosci Biobehav Rev. 2021 May;124:386-404. doi: 10.1016/j.neubiorev.2020.12.005. Epub 2020 Dec 9.},
