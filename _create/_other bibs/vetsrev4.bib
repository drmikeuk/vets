}

@article{wang_physiological_2020,
	title = {Physiological parameter values for physiologically based pharmacokinetic models in food-producing animals. {Part} {II}: {Chicken} and turkey},
	issn = {0140-7783},
	doi = {10.1111/jvp.12931},
	abstract = {Physiologically based pharmacokinetic (PBPK) models are growing in popularity due to human food safety concerns and for estimating drug residue distribution and estimating withdrawal intervals for veterinary products originating from livestock species. This paper focuses on the physiological and anatomical data, including cardiac output, organ weight, and blood flow values, needed for PBPK modeling applications for avian species commonly consumed in the poultry market. Experimental and field studies from 1940 to 2019 for broiler chickens (1-70Â days old, 40Â g - 3.2Â kg), laying hens (4-15Â months old, 1.1-2.0Â kg), and turkeys (1Â day-14Â months old, 60Â g -12.7Â kg) were searched systematically using PubMed, Google Scholar, ProQuest, and ScienceDirect for data collection in 2019 and 2020. Relevant data were extracted from the literature with mean and standard deviation (SD) being calculated and compiled in tables of relative organ weights (\% of body weight) and relative blood flows (\% of cardiac output). Trends of organ or tissue weight growth during different life stages were calculated when sufficient data were available. These compiled data sets facilitate future PBPK model development and applications, especially in estimating chemical residue concentrations in edible tissues to calculate food safety withdrawal intervals for poultry.},
	language = {eng},
	journal = {J Vet Pharmacol Ther},
	author = {Wang, Y. S. and Li, M. and Tell, L. A. and Baynes, R. E. and Davis, J. L. and Vickroy, T. W. and Riviere, J. E. and Lin, Z.},
