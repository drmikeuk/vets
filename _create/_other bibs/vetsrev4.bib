	pages = {104932},
	annote = {1879-1506Stutz, CÃ©lineBatool, FareehaPetit, CatherineStrub, MarionKuchler-Bopp, SabineBenkirane-Jessel, NadiaHuck, OlivierJournal ArticleSystematic ReviewEnglandArch Oral Biol. 2020 Dec;120:104932. doi: 10.1016/j.archoralbio.2020.104932. Epub 2020 Oct 6.},
