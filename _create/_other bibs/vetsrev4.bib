@article{zafra-tanaka_effects_2019,
	title = {Effects of dog-assisted therapy in adults with dementia: a systematic review and meta-analysis},
	volume = {19},
	issn = {1471-244x},
	doi = {10.1186/s12888-018-2009-z},
	abstract = {BACKGROUND: Dog-assisted therapy (DAT) is a non-pharmacological intervention based on the interaction between patients and dogs, which has been proposed to help adults with dementia. However, evidence to support it is lacking. Thus, we aim to evaluate the effects of DAT on this population and to assess the certainty of the evidence of the RCTs estimates. METHODS: A systematic search was performed. We included randomized controlled trials (RCTs) and quasi-experimental (QE) controlled studies published up to March 2018, which evaluated the beneficial and deleterious effects of DAT in adults with dementia. Mean differences (MD) or standardized mean differences (SMD) and their 95\% confidence intervals (95\% CI) were calculated and random effects meta-analyses were performed. Certainty of evidence was assessed for RCTs estimates using the Grading of Recommendations Assessment, Development, and Evaluation (GRADE) methodology. The study protocol has been registered in PROSPERO (CRD42018090434). RESULTS: Ten studies (six RCTs and four QE controlled studies) were eligible for inclusion. Meta-analysis of RCTs showed no effect of DAT in daily life activities (SMD: 0.16; 95\% CI: -0.80 to 1.12), depression (SMD: -0.45; 95\% CI: -2.81 to 1.91), agitation (SDM: -1.12; 95\% CI: -2.67 to 0.43), quality of life (SDM: 0.16; 95\% CI: -0.41 to 0.73), and cognitive impairment (SDM: -0.52; 95\% CI: -1.33 to 0.30), but it found a beneficial effect in apathy (1 study, nâ€‰=â€‰37, MD: 1.81; 95\% CI: 1.26 to 2.36). All outcomes had a very low certainty of evidence according to GRADE methodology. CONCLUSIONS: RCTs evidence of very low certainty suggests that, in adults with dementia, DAT has no effect in daily life activities, depression, agitation, quality of life, and cognitive impairment, although one small study found an apparent beneficial effect in apathy. More well-designed and correctly reported studies are needed in order to provide a conclusion. TRIAL REGISTRATION: CRD42018090434 (PROSPERO).},
	language = {eng},
	number = {1},
	journal = {BMC Psychiatry},
	author = {Zafra-Tanaka, J. H. and Pacheco-Barrios, K. and Tellez, W. A. and Taype-Rondan, A.},
	month = jan,
	year = {2019},
	note = {Edition: 2019/01/27},
	keywords = {Animals, Dogs, Humans, Treatment Outcome, Adult, *Meta-analysis, Randomized Controlled Trials as Topic/methods, *Cognitive dysfunction, interests. PUBLISHERâ€™S NOTE: Springer Nature remains neutral with regard to, jurisdictional claims in published maps and institutional affiliations., Not applicable. COMPETING INTERESTS: The authors declare that they have no competing, *Animal-assisted therapy, Animal Assisted Therapy/*methods, *Dementia, Anxiety/psychology/therapy, Depression/psychology/therapy, Quality of Life/psychology, Cognitive Dysfunction/psychology/therapy, Dementia/*psychology/*therapy},
	pages = {41},
	annote = {1471-244xZafra-Tanaka, Jessica HanaePacheco-Barrios, KevinTellez, Walter AndreeTaype-Rondan, AlvaroOrcid: 0000-0001-8758-0463Journal ArticleMeta-AnalysisSystematic ReviewBMC Psychiatry. 2019 Jan 24;19(1):41. doi: 10.1186/s12888-018-2009-z.},
