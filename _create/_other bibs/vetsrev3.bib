	keywords = {Animals, Livestock, *Dietary Supplements, Animal Husbandry, Mediterranean Region, Plant Extracts/isolation \& purification/*pharmacology, *Animal Feed, *Phytotherapy, *Plants, Medicinal/chemistry/classification, *Salt-Tolerant Plants/chemistry/classification, Animal feed, Ethnobotany, Ethnopharmacology, Halophytes, Nutraceuticals, Phytochemicals/isolation \& purification/*pharmacology, Phytotherapeutics, Veterinary Drugs/isolation \& purification/*pharmacology},
	pages = {113464},
	annote = {1872-7573Oliveira, MartaHoste, HervCustdio, LusaJournal ArticleSystematic ReviewIrelandJ Ethnopharmacol. 2021 Mar 1;267:113464. doi: 10.1016/j.jep.2020.113464. Epub 2020 Oct 10.},
