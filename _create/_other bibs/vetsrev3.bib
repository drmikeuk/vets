@article{moran_nutrition_2018,
	title = {Nutrition and listeriosis during pregnancy: a systematic review},
	volume = {7},
	issn = {2048-6790},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20193199795 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Nutrition+and+listeriosis+during+pregnancy%3A+a+systematic+review.&title=Journal+of+Nutritional+Science&issn=2048-6790&date=2018&volume=7&issue=&spage=e25&aulast=Moran%2C+L.+J.&isbn=&__char_set=utf8},
	abstract = {Listeriosis is a rare but severe foodborne illness which is more common in populations such as pregnant women, and can result in serious complications including miscarriage, prematurity, maternal and neonatal sepsis, and death in the newborn. Population recommendations exist for specific foods and food preparation practices to reduce listeriosis risk during pregnancy. The aim of the present systematic review was to assess the association between listeriosis and these practices during pregnancy to confirm appropriateness of these recommendations. We searched MEDLINE, Embase, CINAHL Plus, Web of Science Core Collection, included articles' references, and contacted clinical experts. All databases were searched until July 2017. Case-control and cohort studies were included which assessed pregnant women or their newborn offspring with known listeriosis status and a nutritional exposure consistent with international population recommendations for minimising listeriosis. Outcomes included listeriosis with or without pregnancy outcomes. Risk of bias was assessed through the Newcastle-Ottawa Scale. Results were described narratively due to clinical heterogeneity in differences in nutritional exposures. Eleven articles comprising case-control or cross-sectional studies met the inclusion criteria. Cases of maternal, fetal or neonate listeriosis were more likely to have consumed high-risk dairy products, meat products or some fruits during pregnancy in comparison with women without listeriosis. Cases of listeriosis were more likely to have consumed foods that are highlighted in population guidelines to avoid to minimise listeriosis in comparison with those without listeriosis during pregnancy. Further research is warranted assessing means of improving the reach, uptake and generalisability of population guidelines for reducing listeriosis during pregnancy.},
	language = {English},
	number = {50},
	journal = {Journal of Nutritional Science},
	author = {Moran, L. J. and Verwiel, Y. and Khomami, M. B. and Roseboom, T. J. and Painter, R. C.},
	year = {2018},
	keywords = {systematic reviews, gestation, pregnancy, research, databases, infections, guidelines, human diseases, sepsis, bacterial diseases, foods, food, relationships, milk products, Meat Produce [QQ030], Milk and Dairy Produce [QQ010], complications, Information and Documentation [CC300], studies, data banks, bacterium, recommendations, Crop Produce [QQ050], man, bacterial infections, bacterioses, foodborne diseases, Listeria (Bacteria), Listeria monocytogenes, meat products, nutrition, neonates, prematurity, dairy products, Food Contamination, Residues and Toxicology [QQ200], Prion, Viral, Bacterial and Fungal Pathogens of Humans [VV210], fruits, women, fetus, listerellosis, listeriosis, case control studies, cohort studies, foetus, food preparation, Human Reproduction and Development [VV060], uptake},
