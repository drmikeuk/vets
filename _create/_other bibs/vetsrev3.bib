	keywords = {Animals, Dogs, Male, Anti-Bacterial Agents/pharmacology, Dog Diseases/epidemiology, Anti-Bacterial Agents, Dog Diseases/microbiology, Dogs/microbiology, Methicillin, Methicillin Resistance/genetics, Methicillin/pharmacology, Staphylococcal Infections/epidemiology, Staphylococcal Infections/veterinary, Staphylococcus/drug effects, Staphylococcus/genetics},
