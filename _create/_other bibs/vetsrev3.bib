	keywords = {Animals, Humans, Prevalence, Swine, Abattoirs, Animal Husbandry, Public Health, Africa, Western/epidemiology, Swine Diseases/*epidemiology/parasitology, Cysticercosis/epidemiology/parasitology/transmission/*veterinary, Epilepsy/complications/epidemiology, Neurocysticercosis/epidemiology/parasitology, Taenia solium/*isolation \& purification},
	pages = {49},
	annote = {1776-1042Melki, JihenKoffi, EugneBoka, MarcelTour, AndrSoumahoro, Man-KoumbaJambou, RonanJournal ArticleReviewParasite. 2018;25:49. doi: 10.1051/parasite/2018048. Epub 2018 Sep 18.},
