	pages = {622},
	annote = {1664-042xRichter, J SebastianQuenardelle, VroniqueRouyer, OlivierRaul, Jean SbastienBeaujeux, RmyGny, BernardWolff, ValrieJournal ArticleReviewFront Physiol. 2018 May 29;9:622. doi: 10.3389/fphys.2018.00622. eCollection 2018.},
}

