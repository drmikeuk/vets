	keywords = {Animals, Dogs, Risk Factors, neoplasms, medical oncology, surgical oncology, Dog Diseases/*surgery, *Margins of Excision, local, margins of excision, neoplasm recurrence, Neoplasm Recurrence, Local/prevention \& control/*veterinary, Sarcoma/surgery/*veterinary},
	pages = {354--364},
	annote = {1476-5829Milovancev, MilanOrcid: 0000-0002-0556-4409Tuohy, Joanne LTownsend, Katy LIrvin, Veronica LJournal ArticleMeta-AnalysisSystematic ReviewEnglandVet Comp Oncol. 2019 Sep;17(3):354-364. doi: 10.1111/vco.12479. Epub 2019 Jun 2.},
