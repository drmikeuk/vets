	keywords = {Animals, Humans, Meningitis, Encephalitis, Anthelmintics/therapeutic use, *Toxocariasis/diagnosis/drug therapy/epidemiology/physiopathology, Albendazole, Nematode, Nervous System Diseases/drug therapy/epidemiology/*parasitology/physiopathology, Nitroimidazoles/therapeutic use, Toxocariasis},
	pages = {291--299},
	annote = {1896-1851Meliou, MariaMavridis, Ioannis NOrcid: 0000-0001-5041-4656Pyrgelis, Efstratios-StylianosAgapiou, EleniJournal ArticleSystematic ReviewSwitzerlandActa Parasitol. 2020 Jun;65(2):291-299. doi: 10.2478/s11686-019-00166-1. Epub 2020 Jan 20.},
