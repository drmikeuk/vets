	journal = {Semina: Ciencias Agrarias},
	author = {Pacheco, R. F. and Filho, D. C. A. and Cattelam, J. and Mayer, A. R. and Burin, M. and Pereira, L. B. and Adams, S. M. and Brondani, I. L.},
