	annote = {1365-2850Prez-Gmez, JorgeOrcid: 0000-0002-4054-9132Amigo-Gamero, HelenaCollado-Mateo, DanielBarrios-Fernandez, SabinaMuoz-Bermejo, LauraGarcia-Gordillo, Miguel ngelCarlos-Vivas, JorgeOrcid: 0000-0002-6377-9950Adsuar, Jos CarmeloOrcid: 0000-0001-7203-3168Journal ArticleReviewEnglandJ Psychiatr Ment Health Nurs. 2020 Nov 10. doi: 10.1111/jpm.12710.},
