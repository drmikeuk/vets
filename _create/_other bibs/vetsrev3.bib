	pages = {245--256},
	annote = {1878-4119Poubel, Victor Lousan do NascimentoSilva, Carolina Amlia BarcellosMezzomo, Luis Andr MendonaDe Luca Canto, GrazielaRivero, Elena Riet CorreaJournal ArticleReviewSystematic ReviewScotlandJ Craniomaxillofac Surg. 2018 Feb;46(2):245-256. doi: 10.1016/j.jcms.2017.11.008. Epub 2017 Nov 20.},
