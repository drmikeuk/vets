	number = {12},
	journal = {PLoS One},
	author = {Palencia, J. Y. P. and Saraiva, A. and Abreu, M. L. T. and Zangeronimo, M. G. and Schinckel, A. P. and Pospissil Garbossa, C. A.},
	year = {2018},
