	keywords = {Animals, Dogs, Meta-analysis, Dog, Histopathology, Leishmania, Clinical condition, Dog Diseases/*parasitology/*pathology, Leishmania/*pathogenicity, Leishmaniasis, Cutaneous/complications/pathology/*veterinary, Leishmaniasis, Visceral/parasitology},
	pages = {916--922},
	annote = {1896-1851Oliveira, Caroline SobotykRatzlaff, Fabiana RaquelPtter, LucianaRomo, Pedro Roosevelt TorresBotton, Snia de AvilaVogel, Fernanda Silveira FloresSangioni, Luis AntonioFinance Code 001/Coordenao de Aperfeioamento de Pessoal de Nvel Superior/Journal ArticleMeta-AnalysisSwitzerlandActa Parasitol. 2019 Dec;64(4):916-922. doi: 10.2478/s11686-019-00063-7. Epub 2019 May 13.},
