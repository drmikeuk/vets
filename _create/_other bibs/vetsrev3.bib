	keywords = {Animals, Goats, Sheep, Prevalence, China, Meta-analysis, Systematic review, China/epidemiology, Brucellosis, Seroepidemiologic Studies, Brucellosis/epidemiology/*veterinary, Goat Diseases/*epidemiology/microbiology, Sheep Diseases/*epidemiology/microbiology, affiliations., authors declare no competing interests. PUBLISHERS NOTE: Springer Nature remains, neutral with regard to jurisdictional claims in published maps and institutional, Not Applicable. CONSENT FOR PUBLICATION: Not Applicable. COMPETING INTERESTS: The, Ovine and caprine},
	pages = {393},
