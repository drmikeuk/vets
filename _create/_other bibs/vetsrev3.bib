	keywords = {Animals, Cattle, Prevalence, Female, Risk Factors, Meta-analysis, Seroepidemiologic Studies, Neospora/*physiology, Cattle Diseases/*epidemiology, Bovine neosporosis, Coccidiosis/epidemiology/*veterinary},
	pages = {1783--1800},
