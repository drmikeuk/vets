	pages = {467--474},
	annote = {1556-7125Muloi, DishonWard, Melissa JPedersen, Amy BFvre, Eric MWoolhouse, Mark E Jvan Bunnik, Bram A DBB/L019019/1/BB\_/Biotechnology and Biological Sciences Research Council/United KingdomMR/R015090/1/MRC\_/Medical Research Council/United KingdomG1100783/MRC\_/Medical Research Council/United KingdomMR/R000093/1/MRC\_/Medical Research Council/United KingdomWT103953MA/WT\_/Wellcome Trust/United KingdomJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewFoodborne Pathog Dis. 2018 Aug;15(8):467-474. doi: 10.1089/fpd.2017.2411. Epub 2018 Apr 30.},
