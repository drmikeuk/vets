	keywords = {Animals, Dogs, Fatty Acids, Dog Diseases/drug therapy, Anti-Inflammatory Agents/therapeutic use, Dermatitis, Atopic/drug therapy/veterinary, Essential/therapeutic use, Histamine Antagonists/therapeutic use},
