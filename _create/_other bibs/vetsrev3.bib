	keywords = {Animals, Dogs, Humans, Female, Male, Pregnancy, Risk Factors, Child, Aged, Adolescent, *Pets, *Human-Animal Bond, *Obesity, *Ownership, *pet ownership, *companion animals, Healthy Lifestyle},
	annote = {1660-4601Miyake, KentaKito, KumikoOrcid: 0000-0002-2138-9794Kotemori, AyakaSasaki, KazutoYamamoto, JunpeiOtagiri, YukoNagasawa, MihoKuze-Arata, SayakaMogi, KazutakaKikusui, TakefumiIshihara, JunkoJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewInt J Environ Res Public Health. 2020 May 17;17(10):3498. doi: 10.3390/ijerph17103498.},
