	journal = {Environ Sci Pollut Res Int},
	author = {Ramirez-Vargas, M. A. and Flores-Alfaro, E. and Uriostegui-Acosta, M. and Alvarez-Fitz, P. and Parra-Rojas, I. and Moreno-Godinez, M. E.},
