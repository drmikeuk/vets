	journal = {PLoS Negl Trop Dis},
	author = {Rostami, A. and Riahi, S. M. and Holland, C. V. and Taghipour, A. and Khalili-Fomeshi, M. and Fakhri, Y. and Omrani, V. F. and Hotez, P. J. and Gasser, R. B.},
	month = dec,
