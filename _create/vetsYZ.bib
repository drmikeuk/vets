	keywords = {Humans, *Diet, Food Handling/methods, Risk, Seafood/analysis, Databases, Factual, Dietary. Fresh fish. Processed fish. Glioma. Meta-analysis., Glioma/*etiology},
	pages = {48--53},
	annote = {1165-158xZhang, ZiheXin, JiangzeJournal ArticleMeta-AnalysisFranceCell Mol Biol (Noisy-le-grand). 2019 Dec 31;65(8):48-53.},
}

