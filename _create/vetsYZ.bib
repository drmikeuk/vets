	keywords = {Animals, Cattle, Humans, Prevalence, Chickens, Food Microbiology, Poultry, Swine, Pigs, Chickens/microbiology, Bacterial, Drug Resistance, Multiple, Colony Count, Microbial, Domestic/microbiology, Sus scrofa/microbiology, Campylobacter/drug effects/isolation \& purification, Health Food/microbiology, Enterobacteriaceae/drug effects/isolation \& purification, Food Industry, Turkeys, Turkeys/microbiology},
	pages = {1217--1232},
