	pages = {4978--4999},
	annote = {1525-3163Fonseca, Pablo Augusto de SouzaDos Santos, Fernanda CarolineLam, StephanieSurez-Vega, AroaMiglior, FilippoSchenkel, Flavio SDiniz, Luiza de Almeida FerreiraId-Lahoucine, SamirCarvalho, Maria Raquel SantosCnovas, AngelaJournal ArticleSystematic ReviewJ Anim Sci. 2018 Dec 3;96(12):4978-4999. doi: 10.1093/jas/sky382.},
