	note = {Edition: 2018/02/07},
	keywords = {Animals, Humans, Rats, Swine, *Disease Models, Animal, Publication Bias, Reproducibility of Results, *Heart Arrest, *Random Allocation, Research Design/*standards},
	pages = {16--21},
	annote = {1873-1570Fabian-Jessing, Bjrn KVallentin, Mikael FSecher, NielsHansen, Frederik BDezfulian, CameronGranfeldt, AsgerAndersen, Lars WJournal ArticleReviewSystematic ReviewIrelandResuscitation. 2018 Apr;125:16-21. doi: 10.1016/j.resuscitation.2018.01.047. Epub 2018 Jan 31.},
