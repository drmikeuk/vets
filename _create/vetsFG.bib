	keywords = {Animals, Cattle, Africa/epidemiology, Mycobacterium bovis, Epidemiology, Europe/epidemiology, Molecular Epidemiology, Zoonoses/epidemiology/microbiology, Databases, Factual, Americas/epidemiology, Asia/epidemiology, *Genotype, Bacterial Typing Techniques, Genetic Loci/*genetics, Minisatellite Repeats/genetics, Miru-vntr, Molecular Typing, Mycobacterium bovis/*genetics/*isolation \& purification/pathogenicity, Spoligotype, Tuberculosis, Bovine/epidemiology/microbiology, Tuberculosis/epidemiology/veterinary},
	pages = {310--315},
	annote = {1096-1208Ghavidel, MahdisMansury, DavoodNourian, KimiyaGhazvini, KiarashJournal ArticleReviewSystematic ReviewEnglandMicrob Pathog. 2018 May;118:310-315. doi: 10.1016/j.micpath.2018.03.036. Epub 2018 Mar 22.},
