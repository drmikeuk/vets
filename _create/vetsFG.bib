	year = {2019},
	note = {Edition: 2020/01/10},
	keywords = {Animals, Humans, Mice, Rats, Rabbits, Anti-Bacterial Agents/*therapeutic use, Wound Healing, Chemotherapy, Adjuvant, Probiotics/*therapeutic use, Cicatrix/prevention \& control, Lactobacillus acidophilus/*physiology, Lactobacillus casei/*physiology, Lactobacillus plantarum/*physiology, Wound Infection/*prevention \& control},
	pages = {7585486},
