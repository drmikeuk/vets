	pages = {403--412},
	annote = {1939-1676Gradner, GabrieleOrcid: 0000-0002-7985-8928Kaefinger, RoseDupr, GillesJournal ArticleSystematic ReviewJ Vet Intern Med. 2019 Mar;33(2):403-412. doi: 10.1111/jvim.15422. Epub 2019 Feb 12.},
