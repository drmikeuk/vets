@article{frank_systematic_2010,
	title = {Systematic review of the use of pheromones for treatment of undesirable behavior in cats and dogs},
	volume = {236},
	issn = {0003-1488},
	url = {http://dx.doi.org/10.2460/javma.236.12.1308},
