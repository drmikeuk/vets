@article{fieldhouse_systematic_2018,
	title = {A systematic review of evidence that enteroviruses may be zoonotic},
	volume = {7},
	issn = {2222-1751},
	doi = {10.1038/s41426-018-0159-1},
	abstract = {Enteroviruses infect millions of humans annually worldwide, primarily infants and children. With a high mutation rate and frequent recombination, enteroviruses are noted to evolve and change over time. Given the evidence that human enteroviruses are commonly found in other mammalian species and that some human and animal enteroviruses are genetically similar, it is possible that enzootic enteroviruses may also be infecting human populations. We conducted a systematic review of the English and Chinese literature published between 2007 and 2017 to examine evidence that enteroviruses may be zoonotic. Of the 2704 articles screened for inclusion, 16 articles were included in the final review. The review of these articles yielded considerable molecular evidence of zooanthroponosis transmission, particularly among non-human primates. While there were more limited instances of anthropozoonosis transmission, the available data support the biological plausibility of cross-species transmission and the need to conduct periodic surveillance at the human-animal interface.},
	language = {eng},
	number = {1},
	journal = {Emerg Microbes Infect},
	author = {Fieldhouse, J. K. and Wang, X. and Mallinson, K. A. and Tsao, R. W. and Gray, G. C.},
	month = sep,
	year = {2018},
	note = {Edition: 2018/09/28},
	keywords = {Animals, Humans, Enterovirus Infections/transmission/*virology, Enterovirus/genetics/*physiology, Zoonoses/transmission/*virology},
	pages = {164},
	annote = {2222-1751Fieldhouse, Jane KOrcid: 0000-0002-5450-0970Wang, XinyeMallinson, Kerry ATsao, Rick WGray, Gregory COrcid: 0000-0002-4628-5908Journal ArticleSystematic ReviewEmerg Microbes Infect. 2018 Sep 26;7(1):164. doi: 10.1038/s41426-018-0159-1.},
