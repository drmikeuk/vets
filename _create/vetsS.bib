	pages = {118813},
	annote = {1879-0631Samadi, MahedehHaghi-Aminjan, HamedSattari, MohammadrezaHooshangi Shayesteh, Mohammad RezaBameri, BehnazArmandeh, MaryamNaddafi, MastourehEghbal, Mohammad AliAbdollahi, MohammadJournal ArticleSystematic ReviewNetherlandsLife Sci. 2021 Jan 15;265:118813. doi: 10.1016/j.lfs.2020.118813. Epub 2020 Dec 1.},
