	pages = {206--218},
	annote = {1095-6808Skvortsova, AleksandrinaVeldhuijzen, Dieuwke SKloosterman, Iris E MMeijer, Onno Cvan Middendorp, HenriëtPacheco-Lopez, GustavoEvers, Andrea W MJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesFront Neuroendocrinol. 2019 Jan;52:206-218. doi: 10.1016/j.yfrne.2018.12.005. Epub 2018 Dec 25.},
