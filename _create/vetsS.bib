	keywords = {Animals, Treatment, Anthelmintics/*therapeutic use, Animals, Wild, Mammals, Parasites, Sarcoptic mange, Macrolides/*therapeutic use, Mite, Parasitic Diseases, Animal/*drug therapy/parasitology},
	pages = {1167--1181},
	annote = {1432-1955Schraven, Andrea LOrcid: 0000-0002-0621-0039Stannard, Hayley JOrcid: 0000-0002-6657-5435Old, Julie MOrcid: 0000-0002-2754-7757Journal ArticleSystematic ReviewGermanyParasitol Res. 2021 Apr;120(4):1167-1181. doi: 10.1007/s00436-021-07092-0. Epub 2021 Feb 22.},
