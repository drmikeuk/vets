	pages = {14420},
	annote = {2045-2322Scharnböck, BettinaRoch, Franz-FerdinandRichter, VeronikaFunke, CarstenFirth, Clair LOrcid: 0000-0002-4919-7786Obritzhauser, WalterBaumgartner, WalterKäsbohrer, AnnemariePinior, BeateJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSci Rep. 2018 Sep 26;8(1):14420. doi: 10.1038/s41598-018-32831-2.},
