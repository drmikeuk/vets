	author = {Scheuermann, L. M. and Gordon-Evans, W. J. and Nault, A. J.},
	month = jan,
	year = {2021},
	note = {Edition: 2020/07/18},
	keywords = {Animals, Dogs, Dog Diseases/surgery/*therapy, Pericardial Effusion/surgery/therapy/*veterinary},
	pages = {20--28},
	annote = {1532-950xScheuermann, Logan MGordon-Evans, Wanda JOrcid: 0000-0003-1356-8327Nault, André JJournal ArticleSystematic ReviewUnited StatesVet Surg. 2021 Jan;50(1):20-28. doi: 10.1111/vsu.13475. Epub 2020 Jul 17.},
