	keywords = {Animals, Mice, Disease Models, Animal, Chronic Disease, *Dose-Response Relationship, Drug, Chagas Disease/*drug therapy, Nitroimidazoles/*administration \& dosage, Trypanocidal Agents/*administration \& dosage},
	pages = {133--140},
