	note = {Edition: 2019/11/19},
	keywords = {Animals, Systematic review, Proteomics, Disease Models, Animal, *Tooth Movement Techniques, *Biological Therapy, Biological therapy, Orthodontic tooth movement},
	pages = {104597},
	annote = {1879-1506Santana, Lucas GarciaDuarte-Rodrigues, LucasAlves-Duarte, Ana CarolineGalvão, Endi LanzaDouglas-de-Oliveira, Dhelfeson WillyaMarques, Leandro SilvaFalci, Saulo Gabriel MoreiraJournal ArticleSystematic ReviewEnglandArch Oral Biol. 2020 Feb;110:104597. doi: 10.1016/j.archoralbio.2019.104597. Epub 2019 Nov 5.},
