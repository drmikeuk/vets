	pages = {466--475},
	annote = {2212-3946Saleh, FatimaItani, LeilaCalugi, SimonaDalle Grave, RiccardoEl Ghoch, MarwanJournal ArticleReviewUnited Arab EmiratesCurr Stem Cell Res Ther. 2018;13(6):466-475. doi: 10.2174/1574888X13666180515160008.},
