	keywords = {Animals, Cattle, Prevalence, Female, Feces/microbiology, Dairying, Canada/epidemiology, Anti-Bacterial Agents/*pharmacology, *antimicrobial resistance, *Drug Resistance, Bacterial, Escherichia coli/*drug effects, Milk/*microbiology, Cattle Diseases/epidemiology/*microbiology, *antibiotic stewardship, *dairy calf, *heifer, Antimicrobial Stewardship, Salmonella enterica/*drug effects},
	pages = {23--34},
