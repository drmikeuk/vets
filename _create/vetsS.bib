	keywords = {Animals, Humans, Female, Pregnancy, Reproduction, Inflammation, Disease Models, Animal, *placenta, Gene Expression Regulation, Parturition, Oxidative Stress, Signal Transduction, *amniochorion, *cervix, *decidua, *MAPK signaling, *myometrium, Genitalia, Female/*metabolism, of interest., p38 Mitogen-Activated Protein Kinases/genetics/*metabolism},
	pages = {e13047},
	annote = {1600-0897Sheller-Miller, SamanthaOrcid: 0000-0002-3991-1747Richardson, LaurenMartin, LauraJin, JinMenon, RamkumarOrcid: 0000-0001-9213-6105R01 HD084532/HD/NICHD NIH HHS/United StatesR03 HD086354/HD/NICHD NIH HHS/United StatesR03HD067446/NIH/NICHD/InternationalJournal ArticleResearch Support, N.I.H., ExtramuralSystematic ReviewAm J Reprod Immunol. 2018 Dec;80(6):e13047. doi: 10.1111/aji.13047. Epub 2018 Sep 4.},
}

@article{shen_bee_2020,
	title = {Bee {Venom} {Acupuncture} for {Shoulder} {Pain}: {A} {Systematic} {Review} and {Meta}-analysis of {Randomized} {Controlled} {Trials}},
	volume = {23},
	issn = {2093-6966 (Print) 2093-6966},
	doi = {10.3831/kpi.2020.23.008},
	abstract = {OBJECTIVES: Our review aimed to summarize and evaluate evidence on the effectiveness of bee venom acupuncture (BVA) in the treatment of shoulder pain. METHODS: Randomized controlled trials (RCTs) evaluating the effectiveness of BVA on shoulder pain were searched up to October 2019 in 11 electronic databases (Medline, Embase, CENTRAL, CiNii, CNKI, VIP, Wanfang, Kmbase, NDSL, RISS, OASIS). The methodological quality of the included RCTs were evaluated using Cochrane Risk of Bias tool and a meta-analysis was performed. RESULTS: Seven studies were included in the review, and four studies were included in the meta-analysis. Comparing BVA plus conventional therapy (CT) with saline injection plus CT, it showed an effect in favor of BVA plus CT in visual analog scale (VAS) and pain rating scale (PRS) (p = 0.02, p = 0.009, respectively). Comparing BVA plus physiotherapy (PT) with saline injection plus PT, it showed that there was no significant difference in VAS and verbal rating scale (VRS) between the two groups. CONCLUSION: This systematic review and meta-analysis suggest that BVA could be beneficial as an adjuvant treatment for shoulder pain.},
	language = {eng},
	number = {2},
	journal = {J Pharmacopuncture},
	author = {Shen, L. and Lee, J. H. and Joo, J. C. and Park, S. J. and Song, Y. S.},
	month = jun,
	year = {2020},
	note = {Edition: 2020/07/21},
	keywords = {meta-analysis, systematic review, bee venom, pharmacopuncture, shoulder pain},
	pages = {44--53},
	annote = {2234-6856Shen, LeiOrcid: 0000-0003-4102-2401Lee, Jong HaOrcid: 0000-0002-2434-8429Joo, Jong CheonOrcid: 0000-0002-4404-0309Park, Soo JungOrcid: 0000-0001-8191-4290Song, Yung SunOrcid: 0000-0002-6318-0585Journal ArticleJ Pharmacopuncture. 2020 Jun 30;23(2):44-53. doi: 10.3831/KPI.2020.23.008. Epub 2020 Jun 10.},
