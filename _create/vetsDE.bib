	keywords = {Animals, Humans, bone regeneration, periodontal regeneration, Models, Animal, Regeneration, Osteogenesis, Stem Cell Transplantation/*methods, Tissue Engineering/methods, Tissue engineering, cell-based therapy, Regenerative Medicine/*methods, *Adipocytes, adipose stem cells, systematic review., *Bone Regeneration, Periodontium/*physiology},
	pages = {504--518},
	annote = {2212-3946Dziedzic, Dilcele Silva MoreiraMogharbel, Bassam FelipeFerreira, Priscila EliasIrioda, Ana Carolinade Carvalho, Katherine Athayde TeixeiraJournal ArticleSystematic ReviewUnited Arab EmiratesCurr Stem Cell Res Ther. 2019;14(6):504-518. doi: 10.2174/1574888X13666181105144430.},
