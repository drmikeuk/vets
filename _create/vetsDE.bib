@article{de_souza_endurance_2019,
	title = {Endurance training on rodent brain antioxidant capacity: {A} meta-analysis},
	volume = {145},
	issn = {0168-0102},
	doi = {10.1016/j.neures.2018.09.002},
	abstract = {The influence of physical exercise on brain antioxidant defense mechanisms has been studied. Nevertheless, the effect of training volume on the brain`s redox balance remains unclear. In this meta-analysis, we compared the effect of training volume on antioxidant enzymatic resource and lipid peroxidation on various brain regions. The activities of the enzymes glutathione peroxidase (GPx), superoxide dismutase (SOD), catalase (CAT) and the levels of thiobarbituric acid reactive substances (TBARS) were also evaluated. The effects of training periods (weeks) and exercise duration were compared. Meta-analysis revealed that protocols over 8 weeks were associated with an increase in SOD (p =  0.0008) and CAT activities (p =  0.0001). Exercise durations for 30 and 60 min were associated with higher CAT activity (p =  0.04). Joint analysis revealed that moderate physical exercise over 4 and 8 weeks promoted a healthy enzymatic balance. However, high volumes of exercise over 8 weeks were associated with the increased antioxidant enzymatic activity, indicating higher reactive oxygen species (ROS) levels. The data also indicated that there is still limited research and inaccurate information, on the safety conditions of training periods that simulate tests of ultra resistance in humans.},
	language = {eng},
	journal = {Neurosci Res},
	author = {de Souza, R. F. and de Moraes, S. R. A. and Augusto, R. L. and de Freitas Zanona, A. and Matos, D. and Aidar, F. J. and da Silveira Andrade-da-Costa, B. L.},
