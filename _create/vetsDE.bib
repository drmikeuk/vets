	pages = {42--50},
	annote = {1532-2661da Silva Xavier, Amanda Azambujada Rosa, Patrícia Pintode Brum Mackmill, LaurettRoll, Victor Fernando ButtowJournal ArticleMeta-AnalysisSystematic ReviewEnglandRes Vet Sci. 2021 Jan;134:42-50. doi: 10.1016/j.rvsc.2020.11.013. Epub 2020 Nov 20.},
