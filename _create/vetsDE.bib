	keywords = {Animals, Cattle, Humans, Prevalence, Food Microbiology, Risk Factors, Risk Assessment, Seasons, Feces/microbiology, Animal Husbandry/methods, Biological, Models, Cattle/microbiology, Colony Count, Escherichia coli O157/growth \& development, Food Contamination/analysis/prevention \& control, Microbial, Monte Carlo Method, Skin/microbiology, Stochastic Processes, Transportation},
