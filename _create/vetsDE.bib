	keywords = {Animals, Humans, Prevalence, Iran, Meta-analysis, Fresh Water, Snail, Iran/epidemiology, Snails/*parasitology, Trematoda/growth \& development/*isolation \& purification, Trematode Infections/epidemiology, Trematodes},
