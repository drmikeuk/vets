	pages = {326--337},
	annote = {1369-1635Marcazzan, SabrinaWeinstein, Roberto LodovicoDel Fabbro, MassimoJournal ArticleReviewSystematic ReviewEnglandPlatelets. 2018 Jun;29(4):326-337. doi: 10.1080/09537104.2017.1327652. Epub 2017 Jun 23.},
