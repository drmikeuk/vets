	keywords = {Animals, Female, Male, *Gastrointestinal Microbiome, *animal model, Host-Pathogen Interactions, Blood Glucose/*metabolism, *microbiota, *diabetes mellitus, *glycaemia, *nutrition, *probiotic, Diabetes Mellitus, Experimental/blood/microbiology/*therapy, Diabetes Mellitus, Type 2/blood/microbiology/*therapy, Dysbiosis, Probiotics/*administration \& dosage},
	pages = {68--79},
	annote = {1365-2613Marques, Anaísa MartinsSarandy, Mariáurea MatiasOrcid: 0000-0003-1313-7877Novaes, Rômulo DiasOrcid: 0000-0002-3186-5328Gonçalves, Reggiani VilelaOrcid: 0000-0002-5831-3590Freitas, Mariella BontempoOrcid: 0000-0001-5132-242xJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewInt J Exp Pathol. 2020 Jun;101(3-4):68-79. doi: 10.1111/iep.12359. Epub 2020 Jul 1.},
