@article{mena-sanchez_dairy_2019,
	title = {Dairy {Product} {Consumption} in the {Prevention} of {Metabolic} {Syndrome}: {A} {Systematic} {Review} and {Meta}-{Analysis} of {Prospective} {Cohort} {Studies}},
	volume = {10},
