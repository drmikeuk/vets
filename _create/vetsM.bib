	keywords = {Animals, Humans, Female, Pregnancy, *Pregnancy, *Systematic review, *Obesity, Motor Activity/*physiology, *Animal Experimentation, *Animal experiments, *Neurobehavior, *Offspring, Behavior, Animal/physiology, Child of Impaired Parents, Obesity/complications/*physiopathology, Pregnancy Complications/*prevention \& control},
	pages = {107--121},
	annote = {1873-7528Menting, Malou Dvan de Beek, CorneliekeMintjens, StijnWever, Kimberley EKorosi, AnikoOzanne, Susan ELimpens, JacquelineRoseboom, Tessa JHooijmans, CarlijnPainter, Rebecca CMC\_UU\_00014/4/MRC\_/Medical Research Council/United KingdomMC\_UU\_12012/4/MRC\_/Medical Research Council/United KingdomJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesNeurosci Biobehav Rev. 2019 Mar;98:107-121. doi: 10.1016/j.neubiorev.2018.12.023. Epub 2019 Jan 3.},
