	pages = {49},
	annote = {1776-1042Melki, JihenKoffi, EugèneBoka, MarcelTouré, AndréSoumahoro, Man-KoumbaJambou, RonanJournal ArticleReviewParasite. 2018;25:49. doi: 10.1051/parasite/2018048. Epub 2018 Sep 18.},
