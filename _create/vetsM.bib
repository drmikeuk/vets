@article{madrigal_use_2018,
	title = {Use of native and non-native shrimp ({Penaeidae}, {Dendrobranchiata}) in world shrimp farming},
	volume = {10},
	issn = {1753-5123},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20193085602 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Use+of+native+and+non-native+shrimp+%28Penaeidae%2C+Dendrobranchiata%29+in+world+shrimp+farming.&title=Reviews+in+Aquaculture&issn=1753-5123&date=2018&volume=10&issue=4&spage=899&aulast=Madrigal%2C+R.+F.+de+A.+G.&isbn=&__char_set=utf8},
	abstract = {The 2013 global farmed shrimp production totalled 4.3 million tons, whereas the production of Litopenaeus vannamei as non-native species represented the 64\% of that amount. The risks of introducing non-native or exotic species are a growing environmental concern. Therefore, the objective of this study was to discuss updated information about the introduction of non-native species and to evaluate the efficiency indicators of non-native species in contrast to native ones. The results of a systematic revision of the publications about shrimp farming are presented, comparing the zootechnical performance of native and non-native species, with the use of meta-analysis tools. The conducted search returned 4680 results, corresponding to the articles that included one of the criteria previously established by the present work. Seven publications, which included 22 studies, met the requirements for the meta-analysis. The effect size calculated was positive, meaning that, according to the considered data, the use of non-native species resulted generally in higher values of growth rate in relation to the native species observed. Other aspects like the emergence and dissemination of the main viral diseases affecting global shrimp farming were also revised. We conclude that, although the advantages of using non-native species, especially L. vannamei, over native species are clear, there are enough compelling evidences to support the viability of small-scale native species farming. We also discussed why big companies, governments and associations related to the production and commerce of farmed shrimp should finance and support the development of native species farming.},
	language = {English},
	number = {4},
	journal = {Reviews in Aquaculture},
	author = {Madrigal, R. F. de A. G. and Silva, U. de A. T. da and Tavares, C. P. dos S. and Ballester, E. L. C.},
	year = {2018},
	keywords = {growth rate, meta-analysis, publications, research, data analysis, aquaculture, aquatic animals, aquatic organisms, viral diseases, taxonomy, endemic species, native species, emerging infectious diseases, Information and Documentation [CC300], studies, Aquaculture (Animals) [MM120], aquatic species, revisions, taxonomic revisions, Prion, Viral, Bacterial and Fungal Pathogens of Animals [LL821], exotic organisms, exotic species, introduced organisms, introduced species, non-indigenous organisms, non-indigenous species, non-native organisms, non-native species, nonindigenous organisms, nonindigenous species, indicators, viral infections, Animal Ecology [ZZ332], aquatic invertebrates, shellfish, emerging diseases, systematics, farming, Taxonomy and Evolution [ZZ380], endemic organisms, companies, corporations, Decapoda, emerging infections, finance, Investment, Finance and Credit [EE800], Litopenaeus, Litopenaeus vannamei, Penaeidae, Penaeus, prawns, shellfish culture, shellfish farming, shellfish ranching, shrimp culture, shrimp farming, shrimp ranching, shrimps},
	pages = {899--912},
