	pages = {1404--1417},
	annote = {1930-739xMurillo, Anarina LOrcid: 0000-0002-3258-4325Kaiser, Kathryn AOrcid: 0000-0002-6258-4369Smith, Daniel L JrOrcid: 0000-0002-1602-2023Peterson, Courtney MOrcid: 0000-0002-4845-6782Affuso, OliviaTiwari, Hemant KAllison, David BOrcid: 0000-0003-3566-9399KL2 TR001419/TR/NCATS NIH HHS/United StatesR01 AG033682/AG/NIA NIH HHS/United StatesT32 DK062710/DK/NIDDK NIH HHS/United StatesP30DK056336/DK/NIDDK NIH HHS/United StatesP30 DK056336/DK/NIDDK NIH HHS/United StatesT32 HL072757/HL/NHLBI NIH HHS/United StatesJournal ArticleResearch Support, N.I.H., ExtramuralSystematic ReviewObesity (Silver Spring). 2019 Sep;27(9):1404-1417. doi: 10.1002/oby.22511. Epub 2019 Jul 30.},
