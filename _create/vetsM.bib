	keywords = {Animals, Humans, Anti-Bacterial Agents/pharmacology, *systematic review, *Food Microbiology, Microbial Sensitivity Tests, *Drug Resistance, Multiple, Bacterial, *antimicrobial resistance, *humans, *Escherichia coli, *food animals, Escherichia coli Infections/microbiology/*transmission/veterinary, Escherichia coli/*drug effects/isolation \& purification},
	pages = {467--474},
	annote = {1556-7125Muloi, DishonWard, Melissa JPedersen, Amy BFèvre, Eric MWoolhouse, Mark E Jvan Bunnik, Bram A DBB/L019019/1/BB\_/Biotechnology and Biological Sciences Research Council/United KingdomMR/R015090/1/MRC\_/Medical Research Council/United KingdomG1100783/MRC\_/Medical Research Council/United KingdomMR/R000093/1/MRC\_/Medical Research Council/United KingdomWT103953MA/WT\_/Wellcome Trust/United KingdomJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewFoodborne Pathog Dis. 2018 Aug;15(8):467-474. doi: 10.1089/fpd.2017.2411. Epub 2018 Apr 30.},
}

@article{munstedt_bee_2019,
	title = {Bee products and the treatment of blister-like lesions around the mouth, skin and genitalia caused by herpes viruses-{A} systematic review},
	volume = {43},
	issn = {0965-2299},
	doi = {10.1016/j.ctim.2019.01.014},
	abstract = {OBJECTIVES: To assess the value of bee products with respect to antiviral efficacy against herpes viruses. DESIGN: A systematic review was done using the JUSTfind System of the Justus-Liebig-University Gießen and Scopus. RESULTS: Three trials on honey and 6 trials on propolis were conducted. Each trial provided evidence that these two bee products are interesting alternatives to acyclovir, especially propolis, which was found to be superior to acyclovir in 4 trials. CONCLUSIONS: The evidence from these trials suggests that propolis is the best of all natural possibilities in the treatment of herpetic skin lesions, especially those related to HSV-1. Future studies should analyse if propolis could be an adjunct to treatment with acyclovir. For lesions in the oral cavity, honey could be an interesting alternative.},
