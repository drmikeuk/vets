@article{miyake_association_2020,
	title = {Association between {Pet} {Ownership} and {Obesity}: {A} {Systematic} {Review} and {Meta}-{Analysis}},
	volume = {17},
	issn = {1661-7827 (Print) 1660-4601},
	doi = {10.3390/ijerph17103498},
	abstract = {Obesity is a major risk factor for lifestyle-related diseases, including cardiovascular disease, type 2 diabetes, and hypertension. Several studies have investigated the association between pet ownership and obesity, but the findings have been inconsistent. This systematic literature review and meta-analysis assessed the association between pet ownership and obesity. Using PubMed and Scopus, we overviewed the literature published until December 2019 and selected pertinent data for meta-analysis. Two independent reviewers extracted the data. Pooled relative risks (RRs) with 95\% confidence intervals (CIs) for obesity were calculated using the random-effects model with inverse-variance weighting. The 21 included articles were cross-sectional studies. Five publications (nine analyses) that reported adjusted RRs for BMI ≥ 25 were included in the meta-analysis. No significant association existed between pet ownership and obesity (pooled RR = 1.038; 95\% CI, 0.922-1.167; I(2) = 51.8\%). After stratification by age group (children vs. adults), no significant association was detected (pooled RR = 0.844; 95\% CI, 0.604-1.179; I(2) = 64.1\% vs. pooled RR = 1.099; 95\% CI, 0.997-1.212; I(2) = 25.2\%). Similarly, no significant association was observed between dog ownership and obesity, indicating no association between pet ownership and obesity. However, no infer causation can be reported because all studies included in this meta-analysis were cross-sectional. Therefore, further prospective studies are needed.},
	language = {eng},
	number = {10},
	journal = {Int J Environ Res Public Health},
	author = {Miyake, K. and Kito, K. and Kotemori, A. and Sasaki, K. and Yamamoto, J. and Otagiri, Y. and Nagasawa, M. and Kuze-Arata, S. and Mogi, K. and Kikusui, T. and Ishihara, J.},
	month = may,
	year = {2020},
	note = {Edition: 2020/05/21},
	keywords = {Animals, Dogs, Humans, Female, Male, Pregnancy, Risk Factors, Child, Aged, Adolescent, *Pets, *Human-Animal Bond, *Obesity, *Ownership, *pet ownership, *companion animals, Healthy Lifestyle},
	annote = {1660-4601Miyake, KentaKito, KumikoOrcid: 0000-0002-2138-9794Kotemori, AyakaSasaki, KazutoYamamoto, JunpeiOtagiri, YukoNagasawa, MihoKuze-Arata, SayakaMogi, KazutakaKikusui, TakefumiIshihara, JunkoJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewInt J Environ Res Public Health. 2020 May 17;17(10):3498. doi: 10.3390/ijerph17103498.},
}

@article{moazeni_epidemiology_2019,
	title = {Epidemiology of taeniosis, cysticercosis and trichinellosis in {Iran}: {A} systematic review},
	volume = {66},
