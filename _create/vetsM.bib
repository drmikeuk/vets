	keywords = {Animals, Humans, Mice, Rabbits, Disease Models, Animal, osteoarthritis, injections, clodronate, clodronic acid, Clodronic Acid/*administration \& dosage/therapeutic use, diphosphonates, Injections, Intra-Articular, intra-articular, Osteoarthritis/*drug therapy},
	annote = {1422-0067Moretti, AntimoOrcid: 0000-0002-4598-2891Paoletta, MarcoLiguori, SaraIlardi, WalterSnichelotto, FrancescoToro, GiuseppeGimigliano, FrancescaIolascon, GiovanniJournal ArticleReviewInt J Mol Sci. 2021 Mar 7;22(5):2693. doi: 10.3390/ijms22052693.},
