	keywords = {Animals, Dose-Response Relationship, Radiation, Macaca mulatta, Acute Radiation Syndrome/*etiology/pathology/therapy, Gastrointestinal Diseases/*etiology/pathology/therapy, Gastrointestinal Tract/pathology/radiation effects},
	pages = {305--338},
	annote = {1538-5159MacVittie, Thomas JFarese, Ann MParker, George AJackson, William 3rdBooth, CatherineTudor, Gregory LHankey, Kim GPotten, Christopher SJournal ArticleResearch Support, N.I.H., ExtramuralResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesHealth Phys. 2019 Mar;116(3):305-338. doi: 10.1097/HP.0000000000000903.},
