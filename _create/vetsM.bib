	year = {2016},
	note = {Edition: 2015/10/29},
	keywords = {Cattle, Prevalence, Risk Factors, Brucellosis},
	pages = {671--685},
	annote = {BioMed Central Journals, CAB Direct (CABI), Cochrane Library – Cochrane, Database of Systematic Reviews (Wiley), ScienceDirect, ERIC plus Text (ProQuest), IBSS (CSA), PubMed, and Web of Science (ISI) Zetoc searched},
