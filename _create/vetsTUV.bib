	annote = {1422-0067Toro, Mario DOrcid: 0000-0001-7152-2613Nowomiejska, KatarzynaAvitabile, TeresioRejdak, RobertTripodi, SarahPorta, AlessandroReibaldi, MicheleFigus, MicheleOrcid: 0000-0003-2243-9033Posarelli, ChiaraOrcid: 0000-0003-0222-5177Fiedorowicz, MichalOrcid: 0000-0001-5755-3608Journal ArticleSystematic ReviewInt J Mol Sci. 2019 Jul 17;20(14):3503. doi: 10.3390/ijms20143503.},
