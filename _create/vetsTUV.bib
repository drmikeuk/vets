@article{vinkeles_melchers_standardisation_2020,
	title = {Standardisation of lymphatic filariasis microfilaraemia prevalence estimates based on different diagnostic methods: a systematic review and meta-analysis},
	volume = {13},
	issn = {1756-3305},
	doi = {10.1186/s13071-020-04144-9},
	abstract = {BACKGROUND: Lymphatic filariasis (LF) infection is generally diagnosed through parasitological identification of microfilariae (mf) in the blood. Although historically the most commonly used technique for counting mf is the thick blood smear based on 20 µl blood (TBS20), various other techniques and blood volumes have been applied. It is therefore a challenge to compare mf prevalence estimates from different LF-survey data. Our objective was to standardise microfilaraemia (mf) prevalence estimates to TBS20 as the reference diagnostic technique. METHODS: We first performed a systematic review to identify studies reporting on comparative mf prevalence data as measured by more than one diagnostic test, including TBS20, on the same study population. Associations between mf prevalences based on different diagnostic techniques were quantified in terms of odds ratios (OR, with TBS20 blood as reference), using a meta-regression model. RESULTS: We identified 606 articles matching our search strategy and included 14 in our analyses. The OR of the mf prevalences as measured by the more sensitive counting chamber technique (≥ 50 µl blood) was 2.90 (95\% confidence interval (CI): 1.60-5.28). For membrane filtration (1 ml blood) the OR was 2.39 (95\% CI: 1.62-3.53), Knott's technique it was 1.54 (95\% CI: 0.72-3.29), and for TBS in ≥ 40 µl blood it was 1.37 (95\% CI: 0.81-2.30). CONCLUSIONS: We provided transformation factors to standardise mf prevalence estimates as detected by different diagnostic techniques to mf prevalence estimates as measured by TBS20. This will facilitate the use and comparison of more datasets in meta-analyses and geographic mapping initiatives across countries and over time.},
	language = {eng},
	number = {1},
	journal = {Parasit Vectors},
	author = {Vinkeles Melchers, N. V. S. and Coffeng, L. E. and de Vlas, S. J. and Stolk, W. A.},
	month = jun,
	year = {2020},
	note = {Edition: 2020/06/13},
	keywords = {Animals, Humans, Prevalence, Odds Ratio, Blood film, Blood smear, Blood/*parasitology, Clinical Laboratory Techniques/*methods/*standards, Counting chamber, Diagnostic comparison, Elephantiasis, Filarial/*blood/*diagnosis, Knott’s technique, Lymphatic filariasis, Membrane filtration, Meta-regression, Microfilariae/*isolation \& purification},
	pages = {302},
	annote = {1756-3305Vinkeles Melchers, Natalie V SOrcid: 0000-0002-1494-3101Coffeng, Luc Ede Vlas, Sake JStolk, Wilma AOPP1184344/Bill and Melinda Gates Foundation/016.Veni.178.023/Nederlandse Organisatie voor Wetenschappelijk Onderzoek/AID-OAA-G14-00010/United States Agency for International Development/Journal ArticleMeta-AnalysisSystematic ReviewParasit Vectors. 2020 Jun 11;13(1):302. doi: 10.1186/s13071-020-04144-9.},
}

@article{virgen-carrillo_diagnostic_2021,
	title = {Diagnostic criteria for metabolic syndrome in diet-induced rodent models: {A} systematic review},
	issn = {1573-3998},
	doi = {10.2174/1573399817666210414103730},
	abstract = {BACKGROUND: Thousands of publications in recent years have addressed the induction of metabolic syndrome (MetS) in rodents, however, the criteria and the reference values for diagnosing this disease have not been defined. OBJECTIVE: Our main objetive was to carry out a systematic review to gather evidence about the criteria for biochemical and anthropometric parameters in which scientific studies have relied to report that rats developed MetS from a previous dietary manipulation. METHODS: We compiled characteristics and findings of diet induced MetS with high-fat, high-carbohydrate, high-fat/high-carbohydrates and cafeteria diet from PubMed and Science Direct databases published in the last 5 years. RESULTS: The results on the principal determinants for the syndrome, published in the reviewed articles, were chosen to propose reference values in the rat models of food induction. CONCLUSION: The values obtained will serve as reference cut-of points in the development of the disease; in addition, the compilation of data will be useful in planning and executing research protocols in animal models.},
	language = {eng},
	journal = {Curr Diabetes Rev},
	author = {Virgen-Carrillo, C. A. and de Los Ríos, D. L. H. and Torres, K. R. and Moreno, A. G. M.},
	month = apr,
