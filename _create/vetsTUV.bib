@article{vasiman_mobile_2019,
	title = {Mobile phone devices and handheld microscopes as diagnostic platforms for malaria and neglected tropical diseases ({NTDs}) in low-resource settings: a systematic review, historical perspective and future outlook. ({Special} {Issue}: {Highlighting} opertional and implementation research for control of helminthiasis.)},
	volume = {103},
	issn = {0065-308X},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba7&AN=20193221268 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Mobile+phone+devices+and+handheld+microscopes+as+diagnostic+platforms+for+malaria+and+neglected+tropical+diseases+%28NTDs%29+in+low-resource+settings%3A+a+systematic+review%2C+historical+perspective+and+future+outlook.&title=Advances+in+Parasitology&issn=0065-308X&date=2019&volume=103&issue=&spage=151&aulast=Vasiman%2C+A.&isbn=&__char_set=utf8},
	abstract = {The accurate, rapid, and cost-effective diagnosis of malaria and neglected tropical diseases (NTDs) in low-resource settings may benefit by significant technological advances in handheld and mobile phone microscopy. We systematically review the available literature in this field and discuss the future directions in which these technologies may be applied. English-language studies from the PubMed, Embase, and Web of Sciences were searched through April 2018 for observational and interventional studies reporting diagnostic characteristics of handheld and mobile phone microscopy devices as compared to field-established gold standard reference tests. Seventeen studies were included in the analysis. Findings included the high performance of the Newton Nm1 microscope in the diagnosis of Plasmodium species, Schistosoma mansoni, and soil-transmitted helminths (STHs), exhibiting sensitivity and specificity values often greater than 90\%. Similarly, the CellScope was shown to have excellent diagnostic characteristics in the detection of Loa loa and Schistosoma species. Fluorescent microscopy was found to have high specificity and sensitivity in the diagnosis of Plasmodium species. Mobile phone technologies and handheld microscopes hold significant promise in the rapid and effective diagnosis of malaria and NTDs in areas where accurate diagnosis is vital. Although many of these technologies have yet to be securely embedded within the health system and studied directly in this context, the foundations for significant healthcare advances and impact have already been laid by several studies conducted within the last decade.},
	language = {English},
	journal = {Advances in Parasitology},
	author = {Vasiman, A. and Stothard, J. R. and Bogoch, I. I.},
	year = {2019},
	keywords = {detection, diagnosis, diagnostic techniques, epidemiology, infections, protozoal infections, parasites, parasitoses, helminths, tropics, mobile telephones, malaria, Techniques and Methodology [ZZ900], Nematoda, Diagnosis of Animal Diseases [LL886], nematodes, Protozoa, protozoal diseases, parasitic diseases, parasitic infestations, parasitic worms, parasitosis, Protozoan, Helminth and Arthropod Parasites of Humans [VV220], Protozoan, Helminth, Mollusc and Arthropod Parasites of Animals [LL822], Schistosoma, parasitology, Schistosoma mansoni, Diagnosis of Human Disease [VV720], tropical diseases, Plasmodium, neglected tropical diseases, tropical countries, tropical zones, African eyeworm, history, History and Biography [BB500], Loa, Loa loa, Onchocercidae, telephones},
