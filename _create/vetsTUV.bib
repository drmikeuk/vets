	pages = {779--793},
	annote = {1432-5195Veronesi, FrancescaVandenbulcke, FilippoAshmore, KevinDi Matteo, BerardoNicoli Aldini, NicolòMartini, LuciaFini, MilenaKon, ElizavetaJournal ArticleResearch Support, Non-U.S. Gov'tSystematic ReviewGermanyInt Orthop. 2020 Apr;44(4):779-793. doi: 10.1007/s00264-020-04493-1. Epub 2020 Feb 5.},
