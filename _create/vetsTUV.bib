	keywords = {Animals, *Chagas disease, *Trypanosoma cruzi, *encephalon, *genotypes, *Mammals, *spinal cord, Chagas Disease/immunology/parasitology/*veterinary, Trypanosoma cruzi/*physiology},
	pages = {983--1005},
	annote = {1469-8161Villalba-Alemán, EvaristoSarandy, Mariáurea MatiasMorais-Santos, MônicaNovaes, Rômulo DiasOrcid: 0000-0002-3186-5328Gonçalves, Reggiani VilelaOrcid: 0000-0002-5831-3590Journal ArticleSystematic ReviewEnglandParasitology. 2019 Jul;146(8):983-1005. doi: 10.1017/S0031182019000210. Epub 2019 Mar 15.},
