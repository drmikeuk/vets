	note = {Edition: 2018/05/29},
	keywords = {Animals, Female, Eating, Lactation, *Lactation, Swine/*physiology, Body Temperature, Feed intake, Heat stress, Pig, Hot Temperature/*adverse effects, Databases, Factual, Offspring, Respiratory Rate},
	pages = {325--330},
	annote = {Vilas Boas Ribeiro, Bruna PontaraLanferdini, EloizaPalencia, Jorge Yair PérezLemes, Marina Alves GomesTeixeira de Abreu, Márvio Lobãode Souza Cantarelli, ViníciusFerreira, Rony AntonioJournal ArticleMeta-AnalysisEnglandJ Therm Biol. 2018 May;74:325-330. doi: 10.1016/j.jtherbio.2018.04.015. Epub 2018 May 1.},
}

