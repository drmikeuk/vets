	keywords = {Animals, Female, Male, Swine, Weaning, Pigs, Swine Diseases/prevention \& control, Viral Vaccines/administration \& dosage, Weight Gain, Circoviridae Infections/prevention \& control, Circovirus/immunology, Mortality, Swine/growth \& development},
