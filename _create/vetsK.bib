	note = {Edition: 2017/10/13},
	keywords = {Animals, Humans, Randomized Controlled Trials as Topic, Burns/*drug therapy/pathology, Fluid Therapy/*methods, Hemodynamics, Resuscitation/*methods, Ringer's Lactate/therapeutic use, Shock/prevention \& control},
	pages = {320--329},
	annote = {1553-2712Kao, YuanLoh, El-WuiHsu, Chien-ChinLin, Hung-JungHuang, Chien-ChengChou, Yun-YunLien, Chieh-ChunTam, Ka-WaiJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesAcad Emerg Med. 2018 Mar;25(3):320-329. doi: 10.1111/acem.13333. Epub 2017 Nov 11.},
