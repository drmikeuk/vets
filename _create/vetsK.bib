	keywords = {Animals, Humans, Prevalence, Meta-analysis, Systematic review, Seroepidemiologic Studies, Iran/epidemiology, Goats/parasitology, Sheep/parasitology, Fasciola hepatica, Livestock/*parasitology, Fasciola gigantica, Zoonoses/*epidemiology/*parasitology, Cattle/parasitology, Fascioliasis/*epidemiology, Fasciolosis},
	pages = {116--123},
