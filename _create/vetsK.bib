	pages = {96--107},
	annote = {1096-1208Khusro, AmeerAarti, ChiromBarbabosa-Pliego, AlbertoRivas-Cáceres, Raymundo ReneCipriano-Salazar, MoisésJournal ArticleSystematic ReviewEnglandMicrob Pathog. 2018 Dec;125:96-107. doi: 10.1016/j.micpath.2018.09.003. Epub 2018 Sep 6.},
