	author = {Kanaan, R. and Farkas, N. and Hegyi, P. and Soós, A. and Hegyi, D. and Németh, K. and Horváth, O. and Tenk, J. and Mikó, A. and Szentesi, A. and Balaskó, M. and Szakács, Z. and Vasas, A. and Csupor, D. and Gyöngyi, Z.},
	month = jan,
