	pages = {2359--2378},
	annote = {1432-1955Odeniran, Paul OlalekanAdemola, Isaiah OluwafemiJegede, Henry OlanrewajuJournal ArticleMeta-AnalysisReviewGermanyParasitol Res. 2018 Aug;117(8):2359-2378. doi: 10.1007/s00436-018-5958-8. Epub 2018 Jun 14.},
