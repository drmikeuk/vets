	pages = {218--227},
	annote = {1369-1600Oberhofer, JuliaNoori, Hamid ROrcid: 0000-0002-2592-247x668863-SyBil-AA/the European Union's Horizon 2020 Program/International01ZX1311A/Bundesministerium für Bildung und Forschung/International01ZX1503/Bundesministerium für Bildung und Forschung/InternationalJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesAddict Biol. 2019 Mar;24(2):218-227. doi: 10.1111/adb.12588. Epub 2017 Dec 14.},
