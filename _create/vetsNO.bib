	pages = {104592},
	annote = {1879-1506Nibali, LAlmofareh, S ABayliss-Chapman, JZhou, YVieira, A RDivaris, KJournal ArticleSystematic ReviewEnglandArch Oral Biol. 2020 Jan;109:104592. doi: 10.1016/j.archoralbio.2019.104592. Epub 2019 Oct 24.},
