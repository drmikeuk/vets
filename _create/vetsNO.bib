	note = {Edition: 2019/11/02},
	keywords = {Animals, Humans, Treatment Outcome, Wound healing, Phytotherapy, Fibrosis, Epithelial-Mesenchymal Transition/*drug effects, Mesenchymal, Benzoquinones/analysis/therapeutic use, Epithelial, Nigella sativa, Nigella sativa/*chemistry, Plant Extracts/*chemistry/*therapeutic use, Thymoquinone, Wounds and Injuries/*drug therapy/physiopathology},
	pages = {290},
	annote = {1472-6882Nordin, AbidKamal, HaziqYazid, Muhammad DainSaim, AminuddinIdrus, RuszymahFF-2017-020/Universiti Kebangsaan Malaysia/FF-2017-020/AMRUS Medik Sdn Bhd/Journal ArticleMeta-AnalysisSystematic ReviewBMC Complement Altern Med. 2019 Oct 30;19(1):290. doi: 10.1186/s12906-019-2706-2.},
