	keywords = {Animals, Cattle, Goats, Horses, Sheep, Dogs, Prevalence, Female, Rabbits, Meta-analysis, Toxoplasma gondii, Cats, Seroepidemiologic Studies, *Cattle Diseases, *Horse Diseases, *Toxoplasma/immunology, Antibodies, Protozoan, *Cat Diseases, *Dog Diseases, *Goat Diseases/epidemiology, *Sheep Diseases, *Toxoplasmosis, Animal/epidemiology, Algeria, Algeria/epidemiology, Literature databases},
	pages = {101603},
