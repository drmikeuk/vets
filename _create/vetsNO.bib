	pages = {495--503},
	annote = {1532-2661Olsson, Débora CristinaTeixeira, Bianca LuiseJeremias, Talita Da SilvaRéus, Jéssica ContiDe Luca Canto, GrazielaPorporatti, André LuisTrentin, Andrea GonçalvesJournal ArticleReviewEnglandRes Vet Sci. 2020 Nov 24:S0034-5288(20)31081-X. doi: 10.1016/j.rvsc.2020.11.014.},
