	month = nov,
	year = {2020},
	note = {Edition: 2020/11/26},
	keywords = {systematic review, in the collection, analyses, or interpretation of data, in the writing, of the manuscript, or in the decision to publish the results., of the study, emergency preparedness, swine depopulation},
	annote = {2076-2615Arruda, Andréia GOrcid: 0000-0001-7040-9686Beyene, Tariku JKieffer, JustinLorbach, Joshua NMoeller, StevenBowman, Andrew SOrcid: 0000-0002-0738-845320-102/National Pork Board/Journal ArticleReviewAnimals (Basel). 2020 Nov 20;10(11):2161. doi: 10.3390/ani10112161.},
