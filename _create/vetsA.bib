@article{alessio_roughage_2018,
	title = {Roughage sources for {Holstein} cows under experimental feeding conditions in {Brazil} - a meta-analysis},
	volume = {39},
	issn = {1676-546X},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20193428831 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Roughage+sources+for+Holstein+cows+under+experimental+feeding+conditions+in+Brazil+-+a+meta-analysis.&title=Semina%3A+Ciencias+Agrarias+%28Londrina%29&issn=1676-546X&date=2018&volume=39&issue=6&spage=2749&aulast=Alessio%2C+D.+R.+M.&isbn=&__char_set=utf8},
	abstract = {The aim of this study was to evaluate the composition, voluntary intake, and digestibility of the diet of Holstein cows, as well as milk yield and milk composition as a function of different roughage sources used under experimental feeding conditions in Brazil, through meta-analysis. The database of this study consisted of 109 experiments with 424 treatments of 3,903 lactating Holstein cows. The data were obtained from studies published between January 2000 and December 2015, and were evaluated by variance analysis. Lactating dairy cow diets with a single source of roughage other than corn silage resulted in lower voluntary intake, milk yield, and differences in milk composition, compared to diets based on corn silage or the combination of two roughages, probably due to reduced nutrient digestibility. Diets based on corn silage are distinguished by feed efficiency, milk yield, and composition. Diets based on the combination of two roughage sources are similar with respect to milk yield and composition, compared to diets based on corn silage; whereas, diets with a single source of roughage (other than corn silage), even when containing a higher proportion of concentrate, led to reduced voluntary intake, milk yield, and food efficiency due to the lower utilization of the nutrients. Diets based on corn silage in general allow a higher proportion of roughage in the diet due to energy density.},
	language = {English},
	number = {6},
	journal = {Semina: Ciencias Agrarias},
	author = {Alessio, D. R. M. and Velho, J. P. and Silveira, V. C. P. and Knob, D. A. and Busanello, M. and Tambara, A. A. C. and Thaler Neto, A.},
	year = {2018},
	keywords = {Brazil, cattle, cows, dairy cows, meta-analysis, milk yield, statistical analysis, diet, digestibility, cattle feeding, Zea mays, roughage, maize, maize silage, silage, cattle breeds, Dairy Animals [LL110], Animal Nutrition (General) [LL500], Animal Nutrition (Production Responses) [LL520], Mathematics and Statistics [ZZ100], Forage and Feed Products (Non-human) [RR000], statistical methods, milk components, milk composition, milk constituents, corn, efficiency, Forage and Fodder Crops [FF007], Holstein (cattle breed), voluntary intake},
