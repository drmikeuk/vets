	note = {Edition: 2018/12/18},
	keywords = {Animals, Dogs, Humans, Treatment Outcome, *Models, Statistical, Global Health, *Systematic review, *Cost-Benefit Analysis, *Cost-effectiveness model, *Economic evaluation, *Prevention and control, *Rabies, *Vaccine, Dog Diseases/prevention \& control/transmission, Post-Exposure Prophylaxis/*economics/methods, Pre-Exposure Prophylaxis/*economics/methods, Rabies/*economics/epidemiology/*prevention \& control/veterinary},
	pages = {A146--a153},
