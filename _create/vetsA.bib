	pages = {185--193},
	annote = {1573-7438Alessio, Dileta Regina MoroOrcid: 0000-0001-5549-9388Velho, João PedroTambara, Antônio Augusto Cortianade Oliveira Gomes, Ivan PedroKnob, Deise AlineHaygert-Velho, Ione Maria PereiraBusanello, MarcosThaler Neto, AndréJournal ArticleMeta-AnalysisUnited StatesTrop Anim Health Prod. 2020 Jan;52(1):185-193. doi: 10.1007/s11250-019-02005-x. Epub 2019 Jul 12.},
