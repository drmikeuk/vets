	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20193041943 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Pharmacotherapeutic+properties+of+Telfairia+occidentalis+Hook+F.%3A+a+systematic+review.&title=Pharmacognosy+Reviews&issn=0973-7847&date=2018&volume=12&issue=24&spage=238&aulast=Aworunse%2C+O.+S.&isbn=&__char_set=utf8},
	doi = {10.4103/phrev.phrev_12_18},
	abstract = {Globally, there has been a growing interest in medicinal plants by researchers. Telfairia occidentalis Hook F. (Fluted pumpkin) is cucurbitaceous vegetable grown in West Africa, particularly in Nigeria for its leaves and seeds. The curative properties of fluted pumpkin are popular in Nigerian folklore medicine, and several investigators have validated these therapeutic effects using animal models. The aim of this work therefore, was to review publicly available literature on the pharmacotherapeutic activities of T. occidentalis. Searches were performed on PubMed, Scopus, ScienceDirect, and Google Scholar for related studies, with search dates set between 1990 and 2018. A total of 499 articles were retrieved and analyzed, with 38 studies ultimately retained. Studies contained in this review were carried out in Nigeria, across different locations. 13 categories of pharmacological activities for fluted pumpkin were documented after analyzing full texts of the articles retained. T. occidentalis offers myriad of healing properties that can be explored by pharmaceutical industries. As the search for potent drugs from botanical sources continues, there is need for future investigations to isolate and characterize pharmacologically active agents that confer medicinal properties on fluted pumpkin, as well as elucidate the structures of these agents and pathways by which they exert their healing properties.},
	language = {English},
	number = {24},
	journal = {Pharmacognosy Reviews},
	author = {Aworunse, O. S. and Bello, O. A. and Popoola, J. O. and Obembe, O. O.},
	year = {2018},
	keywords = {reviews, models, effects, literature, animal models, medicinal plants, seeds, traditional medicines, medicinal properties, pharmacology, medicine, leaves, Africa, plants, Pesticides and Drugs (General) [HH400], Animal Models of Human Diseases [VV400], drugs, medicines, pharmaceuticals, Nigeria, drug plants, folk medicine, medicinal herbs, Non-food/Non-feed Plant Products [SS200], officinal plants, traditional medicine, Protozoan, Helminth and Arthropod Parasites of Humans [VV220], Protozoan, Helminth, Mollusc and Arthropod Parasites of Animals [LL822], subsaharan Africa, Cucurbita, medical sciences, pharmaceutical products, Pharmacology [VV730], pumpkins, Telfairia, Telfairia occidentalis, West Africa},
