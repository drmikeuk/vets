	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba7&AN=20203393840 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=A+systematic+review+of+Leptospira+in+water+and+soil+environments.&title=PLoS+ONE&issn=1932-6203&date=2020&volume=15&issue=1&spage=e0227055&aulast=Bierque%2C+E.&isbn=&__char_set=utf8},
