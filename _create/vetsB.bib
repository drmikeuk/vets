@article{brassolatti_photobiomodulation_2018,
	title = {Photobiomodulation on critical bone defects of rat calvaria: a systematic review},
	volume = {33},
	issn = {0268-8921},
	doi = {10.1007/s10103-018-2653-z},
	abstract = {Bone defects following trauma represent a high impact on the quality of life of millions of people around the world. The aim of this study was to review photobiomodulation (PBM) action in the treatment of bone critical defects in rat calvaria, related to evaluation of the current protocols applied. One hundred and forty-seven articles related to the subject were found by searching the main databases (Pubmed, Lilacs, Web of Science, and Scopus) considering the period of publication until the year 2017, and only 14 corresponded the inclusion criteria established for this systematic review. The main parameters of the PBM were expressed in Table 1. In addition, it was possible to observe the use of two different wavelengths (red and infrared), which are considered therapeutic. Most of the evaluated articles presented positive results that describe a greater amount of neoformed bone, an increase in collagen synthesis, and a contribution to microvascular reestablishment. However, two studies report no effect on the repair process when the PBM was used. In addition, we observed considerable variations between the values of power, fluence, and total energy, which make it difficult to compare the results presented between the selected studies. It was possible to conclude that the infrared laser was more effective in positively stimulating the bone repair process of critical defects. Furthermore, a discrepancy was found in the parameter values used, which made it difficult to choose the best protocol for the treatment of this type of lesion.},
	language = {eng},
	number = {9},
	journal = {Lasers Med Sci},
	author = {Brassolatti, P. and de Andrade, A. L. M. and Bossini, P. S. and Orth, D. L. and Duarte, F. O. and Dos Anjos Souza, A. B. and Parizotto, N. A. and de Freitas Anibal, F.},
	month = dec,
	year = {2018},
	note = {Edition: 2018/10/07},
	keywords = {Animals, Rats, Bone, *Low-Level Light Therapy, Critical bone defects, Lasers, Photobiomodulation, Skull/*pathology},
	pages = {1841--1848},
	annote = {1435-604xBrassolatti, Patriciade Andrade, Ana Laura MartinsBossini, Paulo SérgioOrth, Daiana LaurenciDuarte, Fernanda OliveiraDos Anjos Souza, Ana BeatrizParizotto, Nivaldo Antoniode Freitas Anibal, FernandaJournal ArticleSystematic ReviewEnglandLasers Med Sci. 2018 Dec;33(9):1841-1848. doi: 10.1007/s10103-018-2653-z. Epub 2018 Oct 6.},
