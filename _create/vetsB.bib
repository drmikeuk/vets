	note = {Edition: 2017/12/31},
	keywords = {Animals, Humans, Insect Repellents/*pharmacology, Babesiosis, Anaplasmosis, Arthropod Vectors/drug effects, Borreliosis, Botanical repellent, Deet, Ehrlichiosis, Integrated vector management, Ir3535, Oils, Volatile/*pharmacology, Picaridin, Tick-Borne Diseases/*prevention \& control/veterinary, Ticks/*drug effects},
	pages = {47--54},
	annote = {1873-6254Benelli, GiovanniPavela, RomanJournal ArticleReviewSystematic ReviewNetherlandsActa Trop. 2018 Mar;179:47-54. doi: 10.1016/j.actatropica.2017.12.025. Epub 2017 Dec 26.},
