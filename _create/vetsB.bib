	note = {Edition: 2020/11/14},
	keywords = {Animals, Cattle, Humans, Female, Risk Assessment, Environmental Monitoring, Turkey, Pakistan, India, *Metals, Heavy/analysis/toxicity, *Milk/chemistry, Contamination level, Contamination sources, Heavy metals, Permissible limits, Raw cow milk},
	pages = {141830},
