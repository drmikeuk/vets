	month = jul,
	year = {2018},
	note = {Edition: 2018/05/08},
	keywords = {Animals, Humans, Evaluation Studies as Topic, Wound Healing/radiation effects, *Low-Level Light Therapy, Burns, Burns/*radiotherapy, Low-level laser therapy, Parameters, Skin/pathology/radiation effects},
	pages = {1159--1169},
	annote = {1435-604xBrassolatti, Patriciade Andrade, Ana Laura MartinsBossini, Paulo SérgioOtterço, Albaiza NicolettiParizotto, Nivaldo AntônioJournal ArticleReviewSystematic ReviewEnglandLasers Med Sci. 2018 Jul;33(5):1159-1169. doi: 10.1007/s10103-018-2526-5. Epub 2018 May 5.},
