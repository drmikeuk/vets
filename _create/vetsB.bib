@article{bokma_risk_2018,
	title = {Risk factors for antimicrobial use in food-producing animals: disease prevention and socio-economic factors as the main drivers?},
	volume = {87},
	issn = {0303-9021},
	url = {https://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=caba6&AN=20183279309 https://nusearch.nottingham.ac.uk/openurl/44NOTUK/44NOTUK?genre=article&atitle=Risk+factors+for+antimicrobial+use+in+food-producing+animals%3A+disease+prevention+and+socio-economic+factors+as+the+main+drivers%3F&title=Vlaams+Diergeneeskundig+Tijdschrift&issn=0303-9021&date=2018&volume=87&issue=4&spage=188&aulast=Bokma%2C+J.&isbn=&__char_set=utf8},
	abstract = {The European Union requests an urgent decrease in antimicrobial use (AMU) in food producing-animals to reduce antimicrobial resistance in animals and humans and safeguard the efficacy of antimicrobials for future generations. The identification of risk factors (RFs) for AMU is essential to obtain a rapid reduction. The aim of this review was to summarize the current knowledge of RFs for AMU in veal calves, pigs and poultry. Thirty-three observational studies were included. Well-identified RFs for an increased AMU are frequent purchase of animals, herd size (large or small depending on the animal species), and a lack of selected biosecurity measures. Also in beef breed calves, more antimicrobials are used than in Holstein calves. AMU is influenced by the farmer, the veterinarian and by the integration. In general, socio-economic RFs are largely unexplored. The causal factors for AMU are multiple and complex, with possible confounding factors and unidentified interactions. Additional knowledge of socio-economic drivers appears particularly urgent to create tailor-made guidelines and awareness campaigns for each sector.},
	language = {English},
	number = {4},
	journal = {Vlaams Diergeneeskundig Tijdschrift},
	author = {Bokma, J. and Dewulf, J. and Deprez, P. and Pardon, B.},
	year = {2018},
	keywords = {cattle, poultry, systematic reviews, swine, disease prevention, antibiotics, beef cattle, pathogens, livestock, drug therapy, bacterial diseases, risk factors, socioeconomics, domestic animals, livestock farming, birds, domesticated birds, Meat Producing Animals [LL120], hogs, pigs, Bacteria, bacterium, antibacterial agents, drug resistance, Pesticide and Drug Resistance [HH410], chemotherapy, Pesticides and Drugs, bacterial infections, bacterioses, Prion, Viral, Bacterial and Fungal Pathogens of Animals [LL821], Prion, Viral, Bacterial and Fungal Pathogens of Humans [VV210], Control [HH405], farmers, socioeconomic aspects, antiinfective agents, antimicrobials, livestock numbers, meat animals, veal calves},
