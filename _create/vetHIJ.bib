	pages = {107--116},
	annote = {1369-1627Huhn, Andrew SBerry, Meredith SDunn, Kelly ER01 DA035246/DA/NIDA NIH HHS/United StatesT32 DA007209/DA/NIDA NIH HHS/United StatesJournal ArticleResearch Support, N.I.H., ExtramuralSystematic ReviewInt Rev Psychiatry. 2018 Oct;30(5):107-116. doi: 10.1080/09540261.2018.1514295. Epub 2018 Dec 6.},
