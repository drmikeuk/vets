	pages = {418--427},
	annote = {1872-7123Hu, MingyueZhang, PingLeng, MinminLi, ChenChen, LiJournal ArticleMeta-AnalysisReviewIrelandPsychiatry Res. 2018 Feb;260:418-427. doi: 10.1016/j.psychres.2017.12.016. Epub 2017 Dec 12.},
