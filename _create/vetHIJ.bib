	note = {Edition: 2019/08/20},
	keywords = {*aging, *animal assisted activity, *Animal assisted therapy, *animal companionship, *health, *mental health, *older adult, *pet ownership},
	pages = {365--377},
	annote = {1545-2301Hughes, Michael JVerreynne, Martie-LouiseOrcid: 0000-0003-2957-5927Harpur, PaulOrcid: 0000-0002-3350-0381Pachana, Nancy AOrcid: 0000-0002-8927-4205Journal ArticleUnited StatesClin Gerontol. 2020 Jul-Sep;43(4):365-377. doi: 10.1080/07317115.2019.1650863. Epub 2019 Aug 17.},
