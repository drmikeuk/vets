	keywords = {Animals, Cattle, Humans, Prevalence, Beef, Public Health, Cysticercosis, West Africa, Taenia saginata, Africa, Western/epidemiology, Cattle Diseases/*epidemiology/parasitology, Livestock/*parasitology, Red Meat/parasitology, Africa, Central/epidemiology, Central Africa, Cysticercosis/epidemiology/*veterinary, Taenia saginata/*isolation \& purification, Taeniasis/epidemiology/*veterinary, Tapeworm},
	pages = {324},
