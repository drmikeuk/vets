	keywords = {Animals, Humans, Dietary Supplements, Recovery of Function/*drug effects, *alpha-tocopherol, *ascorbic acid, *spinal cord injuries, Ascorbic Acid/*therapeutic use, Spinal Cord Injuries/*drug therapy/physiopathology, Vitamin E/*therapeutic use, Vitamins/*therapeutic use},
	pages = {465--473},
	annote = {1753-4887Hosseini, MostafaSarveazad, ArashBabahajian, AsrinBaikpour, MasoudVaccaro, Alexander RChapman, Jens RYousefifard, MahmoudRahimi-Movaghar, VafaJournal ArticleMeta-AnalysisResearch Support, Non-U.S. Gov'tSystematic ReviewUnited StatesNutr Rev. 2020 Jun 1;78(6):465-473. doi: 10.1093/nutrit/nuz076.},
